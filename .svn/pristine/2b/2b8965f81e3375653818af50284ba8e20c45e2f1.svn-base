                            var today_str = moment(today).format("YYYY-MM-DD");
                            var arrFrom = T_MIN_1_MON.split('-');
                            var dateFrom = new Date(arrFrom[0] - 0, arrFrom[1] - 0 - 1, arrFrom[2] - 0);
