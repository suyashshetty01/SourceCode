							var VehFullText = (PreRequestCore.vehicle_full).split('|');
							$('.PBMake').html(VehFullText[0]);
							$('.PBModel').html(VehFullText[1]);
							$('.PBVariant').html(VehFullText[2]);
							$('.PBFuel').html(VehFullText[3]);
							$('.PBCC').html(VehFullText[4]);
							$('.VEHRegDate').html((PreRequestCore.vehicle_registration_date).split('-')[0]);
						}
					} else {
						$(".vehicleInfo").hide();
						$(".insurer").empty().show().css('display','flex');
						$(".insurer").append('<p class="loader_newtext">Please fill the vehicle registration number and click on check button</p>')
						$('.Popup_thank').fadeOut();
						$('.VehEligiblePopUp').hide();
						$('.loader').fadeOut();
						////$('.close').fadeIn();
						//$('.errorMsg').fadeIn();
						console.log(data);
						if(data.Msg === Object){
							$('.loader_newtext').html("Cannot Proceed Now. Please Try Again!");
						} else {
							$('.loader_newtext').html(data.Msg);
						}
					}
				},
				error: function (result) {
					console.log(result)
					$(".vehicleInfo").hide();
					$(".insurer").empty().show().css('display','flex');
					$(".insurer").append('<p class="loader_newtext">Please fill the vehicle registration number and click on check button</p>')
					$('.Popup_thank').fadeOut();
					$('.VehEligiblePopUp').hide();
					$('.loader_newtext').html("Cannot Proceed Now. Please Try Again!");
				}
			});
		}
		
		function premium_quote(premiumData){
			if (premiumData){
				if (selected_insurer_id === 0 && is_insurer === false){
					var CC = premiumData.Msg[0]['Cubic_Capacity'];
							var vehicle_cc_slab = 0;
							var arr_cc = [75, 150, 350, 3000];
							for (var k in arr_cc) {
							if (CC < arr_cc[k]) {
									vehicle_cc_slab = arr_cc[k];
									break;
								}
							}
							var netAmount = premiumData.Msg[0]['TP_Premium'];
							var gstAmount = parseFloat(parseInt(netAmount) * 0.18);
							$('.netamount').val(netAmount);
							$('.gstamount').val(gstAmount);
							$('.cubiccapacity').val(vehicle_cc_slab);
							var totalAmount = (parseInt(netAmount)+Math.ceil(gstAmount));
							$('.totalamount').val(totalAmount);
							$('.amount').html('&#8377;'+' '+ totalAmount);
							$('.pay_now').show();
				} else {
				for (var k in premiumData.Msg){
					var tempPremiumData = premiumData.Msg[k];
						if(tempPremiumData.Insurer_Id == selected_insurer_id){
							var CC = tempPremiumData['Cubic_Capacity'];
							var vehicle_cc_slab = 0;
							var arr_cc = [75, 150, 350, 3000];
							for (var k in arr_cc) {
							if (CC < arr_cc[k]) {
									vehicle_cc_slab = arr_cc[k];
									break;
								}
							}
							var netAmount = tempPremiumData['TP_Premium'];
							if(is_tppd === 'yes' && tempPremiumData['TPPD_Premium'] !== '' && (isNaN(tempPremiumData['TPPD_Premium']) === false)){
								netAmount = parseInt(netAmount) - parseInt(tempPremiumData['TPPD_Premium'])
							}
							if(is_pa_od === 'yes' && tempPremiumData['PAOD_Premium'] !== '' && (isNaN(tempPremiumData['PAOD_Premium']) === false)){
								netAmount = parseInt(netAmount) + parseInt(tempPremiumData['PAOD_Premium'])
							}
							var gstAmount = parseFloat(parseInt(netAmount) * 0.18);
							$('.netamount').val(netAmount);
							$('.gstamount').val(gstAmount);
							$('.cubiccapacity').val(vehicle_cc_slab);
							var totalAmount = (parseInt(netAmount)+Math.ceil(gstAmount));
							$('.totalamount').val(totalAmount);
							$('.amount').html('&#8377;'+' '+ totalAmount);
							$('.pay_now').show();
						}
					}
				}
			}
		}

