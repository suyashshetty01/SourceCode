@import "mixins.scss";
@import "header.scss";
@import "globalvar.scss";
 
* { 
    margin: 0;
    padding: 0; 
}     
body { font-family:$Helvetica-400; scroll-behavior: smooth; position: relative; color: $lightenText; overflow-x: hidden;
    &.active { overflow: hidden; position: relative; z-index: 99999;
        &::after { content: ''; position: absolute; left: 0px; right: 0px; top: 0px; height: 100%;
            width: 100%; background: rgba(0, 0, 0, 0.5); z-index: 9;
        }
        // .overlay{display: block;}
    }
    &.modal-active{
        &::after{display: none;}
    } 
    &.notification_active { position: relative; z-index: 99999;
        &::after { content: ''; position: absolute; left: 0px; right: 0px; top: 0px; height: 100%;
            width: 100%; backdrop-filter: blur(0.5px); background: rgba(255, 255, 255, 0.1);
        }
    }  
    &.hidden-scroll {
        &::-webkit-scrollbar-thumb, 
        &::-webkit-scrollbar-track { visibility: hidden; }
    } 
    &::-webkit-scrollbar { width: 5px; height: 5px; }
    &::-webkit-scrollbar-track { box-shadow: inset 0 0 5px rgba(26,129,255,0.6);} 
    &::-webkit-scrollbar-thumb { background: $primarycolor; }
    .overlay-appear{position: absolute; left: 0px; right: 0px; top: 0px; height: 100vh; width: 100%; background: rgba(0, 0, 0, 0.5); z-index: 10; display: none;
        &.active{display: block;}
    }
}      
h1,h2,h3,h4,h5 { font-family:$Helvetica-400; margin: 0;}
a{color: $blackColor;}
span,p,a { font-family: $Helvetica-400; margin: 0;}
b{font-weight: 600;}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button { -webkit-appearance: none; margin: 0;}

ul { margin-bottom: 0px; padding: 0px;
    li { list-style: none; }
}

.min_height500 { min-height: 500px; }

.navigation {
    #sidebar ul li { font-weight: 600; }
    .navbar { justify-content: flex-end; }
}
.btn-hidden{display: none;}    
.navbar .collapse:not(.show) { display: block; }

.navbar-collapse,
.navbar {
    align-items: top!important;
    -ms-flex-align: top!important;
    -moz-flex-align: top!important;
    -webkit-flex-align: top!important;
}
  
.hideclose { display: none; }

.block{display: block;}
a { 
    display: block;
    text-decoration: none;
    &:hover { text-decoration: none; color: $blackColor;}
    &:focus { outline: 0px dotted; outline: 0px auto -webkit-focus-ring-color; }
}

button { cursor: pointer; border-width: 0px;
    &:focus { outline: 0px dotted; outline: 0px auto -webkit-focus-ring-color; }
}
 
.logo {
    a {  display: block; }
    @include tab {
        a { max-width: 280px; }
        &.mobile-logo {
            a {  max-width: 200px; margin: 0 auto 15px auto; }
        }
    }
    @include phone {
        a { max-width: 250px; }
    }
    @include phone-6 {
        a { max-width: 180px; }
    }
}   
.loader {position: fixed; left: 0px; top: 0px; width: 100%; height: 100%; z-index: 9999;
    background: url(../images/loader_icon.gif) 50% 50% no-repeat rgb(255, 255, 255);
}  
   
//Header style  
.mainHeader {position: relative; background: transparent; padding:8px 0px; 
    
    header {
        @include fullwidth;
        .navBlock {    
            .navbarBtn {
                .hamburger {display: none;}
            } 
        } 
        .navbarLink {                    
            right: 100%;
            &.active { left: unset;right: 50%;}
            .navigation-bar {
                li {text-transform:unset; margin: 0; padding: 20px; cursor: pointer;
                    a {font-family: $Helvetica-500; color: $lightenText; padding: 0; font-size: 15px; transition: 0.3s ease-in-out;
                        &:hover { color: $primarycolor;}
                        &.main-btn{padding: 6px 15px; color: $whiteColor;}  
                    }
                    &:first-child { padding-left: 0; }
                    &.active {
                        a { color: $primarycolor; }
                    }      
                    .dropdown-menu{top:90% !important;box-shadow: 0px 3px 10px rgba($blackColor,0.2); overflow: hidden; border-radius: 10px;
                        li{padding: 0px;
                            a{color: $lightenText; width: 100%;transition: none; text-transform: none;
                                &.dropdown-icon-box{flex-direction: row;
                                    .dropdown-text{position: relative; padding-left: 30px; text-align: left; word-break: break-word;
                                        .icon-box{position: absolute; top: -2px; left: 0;  
                                            // transition: $transitionCommon;
                                            img{height: 16px;}
                                            &.hover-icon{opacity: 0;}
                                        }
                                    }
                                }
                            }
                            &.active{background: $primarycolor2; color: $whiteColor;}
                            &:hover{background: $primarycolor;
                                a.dropdown-icon-box .dropdown-text .icon-box{
                                    &.hover-icon{opacity: 1;}
                                    &.normal-icon{opacity: 0;}
                                }
                            }
                        }
                    }
                    &.dropdown-link-box{
                        // display: flex; flex-direction: row; align-items: center;
                        // .dropdown-icon{margin-left: 5px;}
                        a{margin-right: 5px;}
                        &.active{
                            // .icon{filter: invert(34%) sepia(40%) saturate(1132%) hue-rotate(191deg) brightness(90%) contrast(110%)}
                        }
                        .icon{transition: 0.3s ease-in-out;}
                        &:hover{
                            .icon{transform: rotate(180deg);}
                        }
                    }
                    &:hover{ 
                        &.dropdown-link-box .icon{filter: invert(34%) sepia(40%) saturate(1132%) hue-rotate(191deg) brightness(90%) contrast(110%)}
                        a{color: $primarycolor;}
                    }  
                }
            } 
            .main-btn{margin-left: 15px;}
            .nav-btn {
                .main-btn{padding: 6px 15px; font-size: 16px;}
                &:hover{background: none;
                    a{background: $primarycolor; border-radius: 30px; border-color: $primarycolor;color: $whiteColor;}
                }
            }
            .navi_mobile {
                .mobile-logo { display: none; }
            }  
        } 
        .nav-right-box{
            li{display: inline-block; margin-right: 20px;
                &:last-child{margin-right: 0px;}
                .login-btn{font-family: $Helvetica-700; background: $bgPrimary;}
                .language-dropdown{position: relative;
                    .language-dropdown-icon-box{position: absolute; top: 18%; z-index:9; display: inline-block; margin-right: 5px; cursor: pointer;} 
                    .language-dropdown-select{border: 0px;
                        option{padding: 5px;}
                    }
                }
                //Language Dropdown Bootstrap
                .language-dropdown{
                    &.language-dropdown-custom{
                        max-width: 95px; position: relative;
                        .select-dropdown{border: 0px; background: transparent; min-height: auto;
                            &:focus{border: 0px;}
                        }
                        .select-dropdown-box{min-width: auto;}
                        .dropdown-list-row{display: flex; flex-direction: column;
                            .select-dropdown-box-li{margin: 0px;
                                &:hover{background: #003ACC;}
                            }
                        }
                        .language-dropdown-icon-box{position: absolute;top: -1px;z-index:9; display: inline-block; margin-right: 5px; cursor: pointer; }
                        &::after{content: ''; position: absolute; top: 3px; right: 0; background: url(../images/icons/dropdown-arrow.png) center center no-repeat; height: 20px; width: 20px;transform: rotate(0deg); transition: $transitionCommon;}
                        &.active{
                            &::after{transform: rotate(180deg);}
                        }
                    }
                } 
            }
        }
    }
    &.header-shadow{box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.1);
        // header .navbarLink .navigation-bar li .dropdown-menu{top: 100% !important;}
    }
    @include mini-desktop{
        header{
            .navbarLink .navigation-bar li{padding:20px 12px;}
            .nav-right-box li{margin-right: 15px;}
        } 
    } 
    @include tab {
        header {
            .logo{padding-left: 50px;
                &.mobile-logo{padding-left: 0px;}
            }
            .navBlock {
                .navbarBtn {
                    .hamburger { display: block; background: transparent; right: auto; left: 0; }
                }
            }
            .navbarLink { width: 50%; background: $whiteColor; left: auto; padding-top: 15px;
                .navbar-collapse{padding: 30px 0px; flex-direction: column;}
                .navigation-bar li { padding: 0px; margin-bottom: 20px; flex: 0 0 100%; max-width: 100%; width: 100%;
                    &:last-child{
                        a{width: auto;}
                    }
                    &:hover{ 
                        a{color: $lightenText;}
                        &.dropdown-link-box .icon{filter: invert(0%); transform: rotate(0deg);}
                    }  
                    &.active{ 
                        a { color: $primarycolor; }
                        &.dropdown-link-box .icon{filter: invert(34%) sepia(40%) saturate(1132%) hue-rotate(191deg) brightness(90%) contrast(110%); transform: rotate(180deg);}
                    } 
                    .dropdown-menu{margin-top: 5px; min-width: auto; box-shadow: none; max-width: 100%;
                        // max-width: 300px; margin-left: auto; margin-right: auto; 
                        li{margin-bottom: 0px; padding: 5px;
                            a{ width: auto;
                                &.dropdown-icon-box .dropdown-text{text-align: center;}
                            }
                            &:hover{background: $whiteColor; 
                                a{color: $primarycolor !important;
                                    &.dropdown-icon-box .dropdown-text{
                                        .icon-box.hover-icon{opacity: 0;}
                                        .icon-box.normal-icon{opacity: 1;}
                                    } 
                                }
                            }  
                        }
                    }
                } 
                .main-btn{margin-left: 0px;}
                .navi_mobile .mobile-logo { display: block; }
            }
            .nav-box .nav-right-box{
                // li{ margin-right: 0px;
                //     &:last-child{display: none;}
                // }  
            }
            .hamburger{width: 26px; height: 26px;
                &::after{height: 3px;}
                &::before{height: 3px; margin-top: 3px;}
                .bar{height: 3px;}
                &.active{
                    .bar{height: 7px;}
                }
            }
        }
    }
    @include phone {
        header{
            .hamburger::before{margin-top: 6px;}
            .navbarLink{
                .nav-btn{max-width: 200px; margin: 0 auto;}
                // .navigation-bar li .dropdown-menu{max-width: 260px;}
            } 
        }
    }
    @include phone-xs {
        header {
            .navbarLink { width: 70%;
                &.active { right: 30%; }
                .navigation-bar li .dropdown-menu{ 
                    // max-width: 240px;
                    li a{font-size: 14px;}
                } 
            }
            .nav-right-box li{
                .language-dropdown{
                    &.language-dropdown-custom{max-width: 75px;
                        .select-dropdown{font-size: 13px; padding-left: 18px;}
                        .dropdown-list-row .select-dropdown-box-li .select-dropdown-box-link{font-size: 13px;}
                        &::after{top: 0; right: -4px;}
                        .language-dropdown-icon-box{top: -4px; left: 0px;
                            img{height: 14px;}
                        }
                    }
                } 
            }
        }   
    }   
    @include phone-xr{
        header{
            .logo{padding-left: 40px;
                img{height: 40px;}
            }
            .nav-right-box li {
                margin-right: 8px;
            }  
        }  
    }
    @include phone-6{
        header{
            .logo{padding-left: 35px;
                img{height: 30px;}
            }
            .nav-right-box li{margin-right: 2px;
                .language-dropdown .language-dropdown-icon-box{top: 0; left: 5px; pointer-events: none;}
                .language-dropdown{
                    &.language-dropdown-custom{
                        .select-dropdown{font-size: 12px; padding-left: 20px;}
                        .language-dropdown-icon-box{top: -5px; left: 3px;}
                    }
                }   
                .login-btn{padding: 8px 12px;} 
            }  
        }
    }
}    
 
//-----------------------------------------Home Page Style-----------------------------------------------
//Home Banner style 
.home-banner-section{   
    .home-banner-image-box{
        img{height: 390px; object-fit: cover;}
    }
    .banner-detail-area{padding-right: 45px;
        .banner-title{margin-bottom: 15px; font-family: $Jumper-400;
            .heading-inner-text{font-family: $Axure; color: $primarycolor; font-size: 42px; font-weight: 600;}
            .eual-sign{font-weight: 500;}
            .bg-box{ font-family: $Jumper-700; display: inline-block; text-align: center; color: #fff;
                z-index: 1; width: 160px; position: relative;
                &::after{content: ''; display: inline-block; background:url(../images/icons/bg.png) center center no-repeat; background-size: 100% 100%; width: 135px; height: 45px; position: absolute; top: -5px; left: 0px; z-index: -1;}
            } 
        }  
        .get-buddy-box{ 
            .get-buddy-text{margin-bottom: 25px; color: $extraLightText; font-family: $Helvetica-500;}
            .input-area{@include dispflex(flex,flex-start,flex-start); margin-bottom: 20px;
                .input-inner-box{flex: 0 0 320px; max-width: 320px; margin-right: 10px;
                    .home-banner-input{border: 1px solid transparent; box-shadow: 10px 10px 40px rgba(26, 129, 255, 0.1); font-family: $Helvetica-500; 
                        &:focus{border: 1px solid $primarycolor;}
                        &.has-error{border: 1px solid $errorColor;
                            &:focus{border: 1px solid $errorColor;} 
                        } 
                    }   
                }      
                // .find-btn{box-shadow: $btnShadow;}
            }  
            .separator{ 
                .separator-text{display: flex; align-items: center; justify-content: center; margin-bottom: 15px; color: $lightGray; font-family: $Helvetica-500;
                    &::before{content: ''; display: block; width: 20px; height: 1px; background: $lightGray; margin-right: 10px; margin-top:3px;}
                    &::after{content: ''; display: block; width: 20px; height: 1px; background: $lightGray; margin-left: 10px; margin-top:3px;}
                }
            }
        } 
    }  
    @include big-desktop{
        .banner-detail-area { padding-right: 135px; }
    }
    @include mini-desktop{
        .home-banner-image-box{
            img{height: 330px;} 
        }
        .banner-detail-area{padding-right: 20px;
            .banner-title{
                .heading-inner-text{font-size: 34px;}
                .bg-box{width: 130px;
                    &::after{width: 110px; top: -8px; background-size: contain;}
                }
            } 
            .get-buddy-box .input-area{
                .input-inner-box{flex: 0 0 280px; max-width: 280px; margin-right: 8px;}
                .find-btn{padding: 14px 11px;}
            } 
        }
    }
    @include tab{
        .home-banner-image-box{
            img{height: 240px;} 
        }
        .banner-detail-area{padding-right:0px;
            .banner-detail{margin-bottom: 10px;}
            .banner-title{
                .heading-inner-text{font-size: 25px;}
                .bg-box{width: 105px;
                    &::after{width: 100px; top: -10px; left: -5px;}
                }
            } 
            .get-buddy-box{
                .get-buddy-text{margin-bottom: 15px;}
                .input-area{flex-wrap: wrap; margin-bottom: 0px;
                    .input-inner-box{flex: 0 0 100%; max-width: 100%; margin-right:0px;
                        .home-banner-input{margin-bottom: 10px;}
                    }
                    .find-btn{margin-bottom: 10px; padding: 12px 15px;} 
                }
                .separator .separator-text{margin-bottom: 10px;}
            }  
        }     
    }
    @include phone{               
        .home-banner-image-box{text-align: center; margin-bottom: 30px; display: none;
            img{height: 280px;} 
        } 
        .banner-detail-area{text-align: center;
            .get-buddy-box{
                .get-buddy-text{margin-bottom: 30px;}
                .input-area{flex-wrap: nowrap;
                    .input-inner-box{flex: 0 0 65%; max-width: 65%; margin-right: 15px;
                        .home-banner-input{margin-bottom: 0px;}
                    }
                    .find-btn{padding: 17px 20px; margin-bottom: 0px;}
                }
            }  
        }    
    }
    @include phone-xs{ 
        .home-banner-image-box{
            img{height: 250px;} 
        } 
        .banner-detail-area .get-buddy-box{
           
            .input-area{
                .find-btn{flex: 0 0 32%; max-width: 32%; padding: 17px 5px;} 
            }
        }
    }
    @include phone-xr{
        .home-banner-image-box{
            img{height: 230px;} 
        }
    }
    @include phone-6{
        .banner-detail-area .get-buddy-box .input-area{justify-content: space-between;
            .input-inner-box{flex: 0 0 60%; max-width: 60%; margin-right: 8px;}
            .find-btn{padding: 11px 5px; flex: 0 0 37%; max-width: 37%;} 
        } 
    }
    @include phone-5s{
        .home-banner-image-box{
            img{height: 200px;} 
        }
        .banner-detail-area .get-buddy-box .input-area{flex-wrap: wrap; flex-direction: column;
            .input-inner-box{flex: 0 0 100%; max-width: 100%; margin-right: 0px;width: 100%; margin-bottom: 10px;    
            }
            .find-btn{flex: 0 0 100%; max-width: 100%; margin-bottom: 5px; width:100%;} 
        } 
    }
    
} 
            
//Home Slider section style
