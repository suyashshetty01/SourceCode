                    var add_to_cart = require('../models/add_to_cart');
                    let today = new Date();
                    let proposal_id = objInsurerProduct['pg_url'].split('/').pop();
                    let myquery = {PB_CRN: objInsurerProduct['crn'], Ss_Id: objInsurerProduct['ss_id'], Proposal_Id: proposal_id - 0};
