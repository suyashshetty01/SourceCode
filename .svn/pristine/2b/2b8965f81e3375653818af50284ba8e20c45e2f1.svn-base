                                }
                            }
                        }
                        obj_policy['Policy_Status'] = policy_status;
                    } catch (e) {

                    }
                    arrPolicySummary.push(obj_policy);
                }
                var html_report = arrayobjectToHtml(arrPolicySummary);
                res.send(html_report);
            }
        });
    } else {
        res.send('EMPTY_CS_LIST');
