/*----- Tabs -----*/
.tabs {
    width:100%;
    display:inline-block;
}

    /*----- Tab Links -----*/
    /* Clearfix */
    .tab-links:after {
        display:block;
        clear:both;
        content:'';
    }

    .tab-links li {
        margin:0px 1px;
        float:left;
        list-style:none;
        padding:0px !important;
        padding-right:10px !important;
        color:#000;
    }

        .tab-links a {
            padding:4px 0px;
            display:inline-block;
            color:#000;
            font-size:10px;
            text-decoration: none;
            text-transform: uppercase;
            font-family: "Swis721";
            transition:all linear 0.15s;
        }

        .tab-links a:hover {
            color:#e51b21;
            text-decoration:none;
        }

    /*----- Content of Tabs -----*/

        .tab {
            display:none;
        }

        .tab p{
        color:#000 !important;
        font-size:14px !important;
        }


        .viewResult{
            height: 430px;
            overflow: auto;
        }

