							$("#endrsmtNote").text("Policy TAT - 48 hours");
						} else{
							$("#endNote").hide();
						}

                        $('#spn_Category').text(response["Category"]);
                        $('#spn_SubCategory').text(response["SubCategory"]);
                    }
                });
            }
        }

