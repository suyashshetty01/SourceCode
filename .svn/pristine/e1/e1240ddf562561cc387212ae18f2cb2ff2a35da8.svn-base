.hdline{border-left: 1px dotted #3131bd;}

.login-box {
  position: relative;
  border: 1px solid #3131bd;
  margin-top: 90px;
  padding: 25px;
  border-radius: 10px;
  background: rgba(255,255,255,.9);
  box-sizing: border-box;
  box-shadow: 0 3px 10px rgba(0,0,0,.6);
}

.login-box::before {
  position: absolute;
  content: '';
  left: 48%;
  z-index: 10;
  width: 30px;
  height: 15px;
  background:rgba(255,255,255,.9);
  bottom: -1px;
  border-top-left-radius: 50px;
    border-top-right-radius: 50px;
    border: 1px solid #3131bd;
    border-bottom: 0;
}

.login-box::after {
  position: absolute;
  content: '';
  left: 48%;
  z-index: 10;
  width: 30px;
  height: 15px;
  background:rgba(255,255,255,.9);
  top: -1px;
  border-bottom-left-radius: 50px;
    border-bottom-right-radius: 50px;
    border: 1px solid #3131bd;
    border-top: 0;
}
.user-box {
  position: relative;
  margin-top: 30px;
}
.user-box input {
  width: 100%;
  padding: 10px 0;
  padding-bottom: 5px;
  font-size: 14px;
  color: #000;
  margin-bottom: 5px;
  border: none;
  border-bottom: 1px solid #81828C;
  outline: none;
  background: transparent;
}
.user-box label {
  position: absolute;
  top:0;
  left: 20px;
  padding: 00px 0;
  font-size: 14px;
  color: #000;
  pointer-events: none;
  transition: .5s;
}

.user-box input:focus ~ label, .user-box select:focus ~ label{
  top: -25px;
  left: 30px;
  color: #96989A;
  font-size: 12px;
}


.user-box input:valid ~ label, .user-box select:valid ~ label {
  top: -25px;
  left: 20px;
  color: #96989A;
  font-size: 12px;
}

.user-box select{
    width: 100%;
  padding: 10px 0;
  padding-bottom: 5px;
  font-size: 14px;
  color: #000;
  margin-bottom: 5px;
  border: none;
  border-bottom: 1px solid #81828C;
  outline: none;
  background: transparent;
}

.mainBox{text-decoration:none;background: #fff url(../images/schengen-city.png) no-repeat top center; padding-bottom: 20px;}
.btnMain{text-decoration:none;background: #3131bd !important; color:#fff; text-align: center; padding:10px 50px; border: 0px; font-weight: bold; font-size: 22px; border-radius: 5px}
.btnRed{text-decoration:none; background: #d7494d !important; color:#fff; text-align: center; padding:5px 50px; border: 0px; font-weight: bold; font-size: 14px; border-radius: 5px}
.btnBlue{text-decoration:none;background: #3131bd !important; color:#fff; text-align: center; padding:5px 50px; border: 0px; font-weight: bold; font-size: 14px; border-radius: 5px}
.btnGrey{text-decoration:none;background: #96989a !important; color:#fff; text-align: center; padding:5px 50px; border: 0px; font-weight: bold; font-size: 14px; border-radius: 5px}
.btnRed:visited,.btnBlue:visited,.btnGrey:visited{color:#fff;text-decoration:none;}

.overlay {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: opacity 500ms;
  display:none;
  z-index: 100;
}
