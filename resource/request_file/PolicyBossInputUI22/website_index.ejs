    <script>
        $('.modal').on('shown.bs.modal', function (e) {
            $('.product-slider-small').slick('setPosition');
            $('.product-slider').slick('setPosition');
            if($(window).width() < 575){
                setTimeout(function(){
                    $('.features-slider').addClass('visible');
                }, 200);
                $('.features-slider').slick('setPosition');
            }
            $('.product-slider-area').addClass('open');
        });

        function setPlaceholderColour() {
            $('.common-dropdown').css('color','#222')
        }

        function openSelectCarModal(displayType) {
            
            $('#triggerSelectCarDetails').trigger('click');
            $('.SelectCarDetail-modal').hide();
            if (displayType == 'hidden') {
                $('.WithoutCarNumber-modal').hide();
                $('.NewCar-modal').show();
                $('.after-submit-modal').hide();
            }if (displayType == 'not_hidden'){
                $('.NewCar-modal').hide();
                $('.WithoutCarNumber-modal').show();
                $('.after-submit-modal').hide();
            }
            if (displayType == 'ibuddy') {
                $('.NewCar-modal').hide();
                $('.WithoutCarNumber-modal').hide();
                $('.after-submit-modal').show();
            }
        }

        function showSelectCarDetails(modal) {
            $('.'+modal).hide();
            if (modal == 'WithoutCarNumber-modal') {
                $('#card-detail-form').show();
                $('#new-card-detail-form').hide();
                $('#after-submit-card-detail-form').hide();
            }
            if (modal == 'NewCar-modal') {
                $('#new-card-detail-form').show();
                $('#card-detail-form').hide();
                $('#after-submit-card-detail-form').hide();
            }
            if (modal == 'after-submit-modal') {
                console.log('ibuddy clicked');
                // $('#my_iBuddy').hide()
                // $('.WithoutCarNumber-modal').hide();
                // $('.NewCar-modal').hide()
                $('#card-detail-form').hide();
                $('#new-card-detail-form').hide();
                $('#after-submit-card-detail-form').show();
            }
            $('.SelectCarDetail-modal').find('.nav-tabs').each(function () {
                if ($(this).attr('data-for-modal') != modal){
                    $(this).hide();
                }else{
                    $(this).show();
                }
            })
            var tempNavLinkCounter = 0;
            
            $('#selectCarDetails').find('.'+modal+'-nav-tabs').find('.nav-link').each(function () {
                tempNavLinkCounter = tempNavLinkCounter+1;
                if (tempNavLinkCounter == 1) {
                    $(this).addClass('active');
                }else{
                    $(this).removeClass('active');
                }
            })
            if (modal == 'WithoutCarNumber-modal') {
                $('#card-detail-form').find('.tab-pane').each(function () {
                    $(this).removeClass('active');
                });
                $('#card-detail-form').find('#nav-make').addClass('active');
            }
            if (modal == 'NewCar-modal') {
                $('#new-card-detail-form').find('.tab-pane').each(function () {
                    $(this).removeClass('active');
                });
                $('#new-card-detail-form').find('#nav-make-2').addClass('active');
            }
            if (modal == 'after-submit-modal') {
                $('#after-submit-card-detail-form').find('.tab-pane').each(function () {
                    $(this).removeClass('active');
                });
                $('#after-submit-card-detail-form').find('#nav-make-3').addClass('active');

                var tempValues = $('#after-submit-modal-prefiled-input').val()
                    var tempValuesArr = tempValues.split(', ');
                    var CarBran = tempValuesArr[0]
                    var CarMode = tempValuesArr[1]
                    var CarFuelType = tempValuesArr[2]
                    var CarVariant = tempValuesArr[3]

                if($( window ).width() > 768){
                    
                    $('#nav-make-tab-3').text(CarBran);
                    $('#nav-model-tab-3').text(CarMode);
                    $('#nav-fuel-tab-3').text(CarFuelType);
                    $('#nav-variant-tab-3').text(CarVariant);
                    
                }

                $('#nav-make-tab-3').addClass('visited');
                $('#nav-model-tab-3').addClass('visited');
                $('#nav-fuel-tab-3').addClass('visited');
                $('#nav-variant-tab-3').addClass('visited');

                $('#nav-make-3').find('.select-brand-listing-col').each(function(){
                    var tempInpVal = $(this).find('.form-check-input').attr('data-value')
                    if (tempInpVal == CarBran) {
                        $(this).find('.form-check-input').prop('checked', true);
                    }
                })
                $('#nav-model-3').find('.select-model-listing-col').each(function(){
                    var tempInpVal = $(this).find('.form-check-input').attr('data-value')
                    if (tempInpVal == CarMode) {
                        $(this).find('.form-check-input').prop('checked', true);
                    }
