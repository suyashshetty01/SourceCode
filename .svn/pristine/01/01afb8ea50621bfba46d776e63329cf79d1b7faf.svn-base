//Location section
.locations-section{
    .title-text{margin-bottom: 30px; color: $blackColor;}
    .location-section-box{
        .accordion-button[type='button']{appearance: none;}
        .location-zone-title{font-family: $Helvetica-500; position: relative; color: $blackColor; margin-bottom: 40px;
            &::before{content: ''; position: absolute; bottom: 0px; left: 0; right: 0; margin: 0 auto; height: 3px; width: 10%; background: $primarycolor;} 
            &::after{display: none;}
        }
        .location-listing-area{
            .listing-ul{@include dispflex(flex,flex-start,flex-start); flex-direction: row; flex-wrap: wrap; margin: 0 -15px;
