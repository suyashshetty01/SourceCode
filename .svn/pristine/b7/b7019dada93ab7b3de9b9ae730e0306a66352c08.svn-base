                    console.error('Exception in my_cart_pay() for inspectionSchedules db details : ', ex);
                    res.send(ex, dbUsers);
                }
            }
        });
    } catch (e) {
        res.json({'Msg': "my_cart_pay - Exception", 'Status': "Failure"});
    }
