//Claims Listing section
.claims-listing-section{
    .claims-main-area{
        .search-box{max-width: 460px;
            .icon-box{top: 9px; right: 20px;
                img{ height: 20px;}     
            }
        }
        .claims-listing-area{
            .claims-listing-row{background: $whiteColor; box-shadow: 0px 4px 40px rgba(0, 0, 0, 0.1);
                border-radius: 20px; margin-bottom: 30px; overflow: hidden;
                .claims-listing-box{@include dispflex(flex,flex-start,flex-start); flex-direction: row; flex-wrap: wrap; margin: 0 -15px; padding: 15px;
                    .heading-text{color: $lightTextColor; font-family: $Helvetica-500; display: block; margin-bottom: 5px;}
                    .flex-col-3{@include flex_box(4); padding: 0 15px;}
                    .content-box{
                        .description-text{
                            &.contact-number-box{@include dispflex(flex,flex-start,center); flex-wrap: wrap;
                                .number-link{padding:0px 8px; position: relative;display:inline-block; color: $blackColor; font-family: $Helvetica-500;
                                    &::after{content:'/'; position: absolute; top:-2px; left:-4px; font-size:20px; color:#000;}
                                    &:first-child{padding-left:0px;
