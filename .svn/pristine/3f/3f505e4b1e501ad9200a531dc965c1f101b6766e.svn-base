.ErrMsg{display:none}
.ErrorMsg{border:1px solid red !important;}
.ui-helper-hidden-accessible{display: none}
@import "mixins.scss";
.main-btn { display: inline-block; font-family: $Helvetica-700; color: $whiteColor; background: $primarycolor; font-size: 18px; box-shadow: 10px 10px 20px rgba(26, 129, 255, 0.15);
     box-shadow: $btnShadow;
    padding: 17px 20px; text-align: center; border-radius:15px; transition: $transitionCommon;
    &.btn-icon{display: flex; flex-direction: row; justify-content: center; align-items: center;
        .icon-box{margin-right: 5px;}
    }
    &:hover { color: $whiteColor; background: $primaryDark; box-shadow: none}
    &.btn-icon{display: flex; flex-direction: row; align-items: center;
        .prev{margin-right: 5px;}
        .next{margin-left:5px}
    }
    &.width-full{width: 100%;}
    @include mini-desktop { padding: 14px 16px; }
    @include tab { padding: 17px 12px; font-size: 18px;
    }
    @include phone-xs { font-size: 16px; padding: 17px 20px;
    }
    @include phone-xr {
        padding: 17px 18px;
    } 
    @include phone-6 {
        padding: 11px 18px; border-radius: 10px;
        // font-size: 15px;
    } 
} 

.main-title{font-size: 48px;
    @include mini-desktop{font-size: 44px;}
    @include tab{font-size: 38px;}
    @include phone{font-size: 32px;}
    @include phone-xs{font-size: 28px;}
    @include phone-xr{font-size: 24px;}
}
.font-40{font-size: 40px;
    @include mini-desktop{font-size: 35px;}
    @include tab{font-size: 30px;}
    @include phone{font-size: 26px;}
    @include phone-xr{font-size: 24px;}
}
.font-36{font-size: 36px;
    @include mini-desktop{font-size: 32px;}
    @include tab{font-size: 28px;}
    @include phone{font-size: 24px;}
}
.font-32{font-size: 32px;
    @include mini-desktop{font-size: 30px;}
    @include tab{font-size: 28px;}
    @include phone{font-size: 25px;}
    @include phone-xr{font-size: 22px;}
}
.font-30{font-size: 30px;
    @include mini-desktop{font-size: 28px;}
    @include tab{font-size: 24px;}
    @include phone{font-size: 22px;}
    @include phone-xr{font-size: 18px;}
}
.font-26{font-size: 26px;
    @include mini-desktop{font-size: 24px;}
    @include tab{font-size: 22px;}
    @include phone{font-size: 20px;}
    @include phone-xr{font-size: 18px;}
    @include phone-6{font-size: 16px;}
    @include phone-5s{font-size: 15px;}
}
.font-24{font-size: 24px;
    @include mini-desktop{font-size: 22px;}
    @include tab{font-size: 20px;}
    @include phone-xs{font-size: 18px;}
    @include phone-6{font-size: 16px;}
}
.font-22{font-size: 22px;
    @include mini-desktop{font-size: 20px;}
}
.font-20{font-size: 20px;
    @include tab{font-size: 18px;}
    @include phone-xr{font-size: 16px;}
}
.font-18{font-size: 18px;
    @include phone-xr{font-size: 16px;}
    @include phone-5s{font-size: 15px;}
}
.font-16{font-size: 16px;
    @include phone-5s{font-size:15px;}
}
.font-15{font-size: 15px;}

.sub-title{font-family: $Helvetica-700; color: $lightenText; text-align: center; margin-bottom: 30px;
    @include tab{margin-bottom: 25px;}
    @include phone{margin-bottom: 20px;}
    @include phone-xs{margin-bottom: 15px;}
}
.title-text{font-family: $Helvetica-700;}
.primary-text{color: $primarycolor;}
.small-detail-text{font-size: 14px; color: #777; font-weight: 400;}
.font-italic{font-style: italic;}
.text-lightgrey{color: $lightGray;}
.text-extralight{color: $extraLightText;}
.error-text-color{color: $errorColor;}
.dark-text{color: $lightenText;}
.light-text{color: $lightenText;}
.weight-600{font-weight: 600;}
.weight-700{font-weight: 700;}
.weight-800{font-weight: 800;}
.dark-text-shadow{color: #0651AD;}
.btn-disable{display: none;}
.highlight-block{display: block; color: $primarycolor; font-weight: 700;
    .bg-green{background:$primarycolor2; display: inline-block; color: $whiteColor; padding: 10px; box-shadow: 2px 2px 0px 2px #07681e; transform: skew(-8deg);}
    @include phone-xs{padding: 5px;}
}
.blend-mode{mix-blend-mode: darken;}
.section-padding { padding: 80px 0;
    @include mini-desktop { padding: 70px 0px; }
    @include tab { padding: 60px 0px; }
    @include phone { padding: 50px 0px; }
    @include phone-xs { padding: 40px 0px; }
}
.pedding-top-0{padding-top: 0px;}
.pedding-bottom-0{padding-bottom: 0px;}
.padding-top-40{padding-top: 40px;
    @include phone-6{padding-top: 30px;}
}

.bg-blue-banner-description{max-width: 1024px; margin: 0 auto;
    @include mini-desktop{max-width: 900px;}
    @include tab{max-width: 600px;}
    @include phone{max-width: 480px;}
    @include phone-xs{max-width: 420px;}
    @include phone-xr{max-width: 375px;}
    @include phone-6{max-width: 320px;}
    @include phone-5s{max-width: 300px;}
}
.mob-order-1{
    @include phone{order: 1;}
}
.mob-order-2{
    @include phone{order: 2;}
}
.login-btn{background: $bgPrimary; color: $whiteColor; padding: 9px 32px; border-radius: 10px; font-size: 15px; box-shadow: $primaryShadow; transition: $transitionCommon;
    // &.posp-login-btn{padding: 9px 20px;
    //     @include mini-desktop{padding: 9px 15px;}
    // }
    &:hover{box-shadow: $primaryShadow40; color: $whiteColor;}
    @include phone{padding: 10px 25px;}
    @include phone-xs{padding: 8px 20px;}
}
.nav-tabs{border: 0px; margin-bottom: 30px;
    .nav-item{margin-right: 10px;}
    .nav-tab-button{border: 0px;border-radius: 30px; font-weight: 600; padding:10px 20px;
        &.active{background: $primarycolor2; color: $whiteColor;}
        &:hover{background: $primarycolor2; color: $whiteColor;}
    }
}
.bg-dark-blue{background: $primarycolor2;}
.bg-light-blue{background: $bgLightBlue;}
.bg-light-primary{background: $bgLightPrimary;}
.bg-light-gradient{background: $bgLightBlueGradient;
    &.home-bg{height: 600px;
        @include phone{height: 680px;}
    }
}
.bg-blue-banner{background: $primarycolor;border-radius: 0px 0px 127px 127px; padding: 50px 0px;
    @include mini-desktop{padding: 40px 0px;}
    @include tab{padding: 30px 0px;}
    @include phone{border-radius: 0px 0px 100px 100px;}
    @include phone-xs{padding: 20px 0px;}
    @include phone-xr{border-radius: 0px 0px 70px 70px;}
    @include phone-6{padding: 15px 0px;}
    @include phone-5s{border-radius: 0px 0px 40px 40px;}
}
.bg-gradient-left-to-right{background: $bgLefToRight;}
.bg-centered-gradient{background: $bgCenteredGradient;}
.bg-blue-with-text-shadow{background: $primarycolor; font-style: italic; padding: 4px 15px; color: $whiteColor; font-family: $Helvetica-800; text-shadow: 2px 2px #0651ad;}
.bg-black-overlay{background: $blackTransparent;}
.common-dropdown{
    &:focus{box-shadow: none;}
}
.flex-100{flex: 0 0 100% !important; max-width: 100% !important;}
.image-wrapper-box{background: $whiteColor; box-shadow: 0px 4px 60px 30px rgba(0, 0, 0, 0.05); border-radius: 20px; padding: 50px; max-width: 525px; margin-right: auto;
    @include big-desktop{max-width: 580px;}
    @include mini-desktop{padding: 40px;}
    @include tab{padding: 30px;}
}
.image-box{text-align: center;}

.flex-box{display: flex; flex-direction: row; align-items: center;
    .flex-50{flex: 0 0 50%; max-width: 50%;}
}
.policy-info-box{
    @include phone-6{
        .radio-box{margin-bottom: 10px;}
    }
}
.policy-info-flex-box{margin: 0 -5px; align-items: flex-end;
    .flex-50{padding: 0 5px;} 
    @include phone-xs{
        .input-field-box .field-icon-box{right: 12px;}
    }
    @include phone-xr{align-items: flex-end;}
}
.policy-information-area{
    @include phone-5s{
        .flex-box{flex-wrap: wrap;
            .flex-50{flex: 0 0 100%; max-width: 100%; margin-bottom: 10px;}
        } 
    }
}
.date-picker-area,.select-area,.previous-policy-select-area,.tp-insurance-select-area{margin-bottom: 15px;}


.slick-dots{display: flex; flex-direction: row;
    li{padding: 0px 3px;
        &:first-child{padding-left: 0px;}
        button{border: 1px solid transparent; width: 8px; height:8px; background: $primaryLight; font-size: 0px; border-radius: 50%; transition: 0.3s ease-in-out;}
        &.slick-active{
            button{border-radius: 5px; width: 20px;background: $primarycolor;}
        }
    }
}
//Help Box
.help-box{
    img{height:22px;}
}

//Hide slick arrow for First and Last slide
.slick-arrow{
    &.slick-disabled{display: none !important;}
}

.border-white-bg-box{background: $whiteColor; border: 2px solid rgba(0, 0, 0, 0.1); border-radius: 10px;}

.read-more-btn{
    .btn-icon{
        img{transform: translateX(0px); transition: $transitionCommon;}
    }
    &:hover{
        .btn-icon img{transform: translateX(4px);}
        
    }
}

//Message Box
.message-box{border-radius: 10px; position: relative; overflow: hidden; padding: 5px 0px 5px 15px; min-height: 65px; margin-bottom: 20px;
    &::before{content: ''; position: absolute; left: 0; top: 0; height: 100%; width: 5px;}
    &.bg-error{background: #FFEBE8;
        &::before{background: $errorColor;}
    }
    .title-message{font-family: $Helvetica-500;}
}

//Search Box
.search-box{position: relative;margin-bottom: 30px;
    .icon-box{position: absolute; top: 10px; right: 15px;}
    .input-field{}
    &.modal-search-box{margin-bottom: 0px;
        .icon-box{top: 8px; right: auto; left: 10px;}
    }
    @include tab{
        .input-field{font-size: 14px; padding: 0 10px;}
        &.modal-search-box{
            .input-field{font-size: 16px;}
        }
    }
    @include phone-xs{
        &.modal-search-box{width: 100%; margin-top: 15px;}
    }
    @include phone-6{
        .icon-box{top: 10px;}
    }
}

.explore-box{ padding: 20px; margin-bottom: 30px;
    .input-field-box{margin-bottom: 15px;}
    .find-btn{border-radius: 10px;}
    @include tab{padding: 12px;
        .input-field{font-size: 14px; padding: 0 10px;}
    }
}
.categories-box{padding: 20px; margin-bottom: 30px;
    .categories-ul{
        .categories-li{margin-bottom: 15px; @include dispflex(flex,flex-start,center);
            .categories-link{font-family: $Helvetica-500;  display: inline-block; transition: $transitionCommon;}
            &::before{content: ''; background: url(../images/icons/chevron-right.png) center center no-repeat; display: inline-block; margin-right: 10px; width: 9px; height: 17px; }
            &:last-child{margin-bottom: 0px;}
        }
    }
    @include tab{padding: 12px;}
}
.recent-post-box{padding: 20px; margin-bottom: 30px;
    .post-ul{
        .post-li{margin-bottom: 20px;
            .post-box{@include dispflex(flex,flex-start,center);
                .post-image-box{flex: 0 0 80px; max-width: 80px; margin-right: 20px; border-radius: 5px; overflow: hidden;
                    img{height: 80px; object-fit: cover;}
                }
                .post-detail-box{
                    .post-title{font-family: $Helvetica-500; margin-bottom: 5px;}
                    .date-text{color: $lightTextColor; font-family: $Helvetica-500; font-size: 14px;}
                }
            }
            &:last-child{margin-bottom: 0px;}
        }
    }
    @include tab{padding: 12px;
        .post-ul .post-li .post-box{flex-direction: column; align-items: flex-start;
            .post-image-box{margin-bottom: 5px; margin-right: 0px;}
        }
    }
}
.right-side-section{
    .title-text{margin-bottom: 10px;}
    .explore-heading{display: inline-block; position: relative;}
    .tool-tip-box{position: absolute; display: inline-block; top: 0; right: -30px; cursor: pointer;}
    .input-field{border: 2px solid rgba(0, 0, 0, 0.1);
        &:focus{border: 2px solid $primarycolor;}
        &.has-error{
            &:focus{border: 2px solid $errorColor;}
        }
    }
    @include phone{display: none;}
}

//Tabs style
.nav-tab-line{
    .nav-item{margin: 0px; padding: 0 25px;
        .nav-link{border: 0px; border-bottom: 2px solid transparent; color: $lightTextColor; padding:10px 0px; transition:$transitionCommon ; font-family: $Helvetica-500;
            &.active{color: $lightenText; border-bottom: 2px solid #FF0000;}
            &:hover{color: $lightenText;}
        }
        &:first-child{padding-left: 0px;}
        &:last-child{padding-right: 0px;}
    }
}

//Breadscrumb
.breadcrumb-area{
    .breadcrumb-list{ background-color: transparent; padding: 0px; margin-bottom: 30px;@include dispflex(flex,flex-start,center); flex-direction: row; flex-wrap: wrap; margin-bottom: 20px;
        .breadcrumb-list-item{@include dispflex(flex,flex-start,center); flex-direction: row;
            .breadscrumb-link{color: $primarycolor; font-family: $Helvetica-500;}
            &::after{ content:""; width:9px; height:13px; margin:0 8px; margin-top: 2px; background:url(../images/icons/breadscrumb-arrow.png) no-repeat center center; display: inline-block;
            }
            &:last-child{
                &::after{display: none;}
            }
            &.active{
                .breadscrumb-link{color: $extraLightText;}
            }
        }
    }
    .mob-breadcrumb{display: none}
        @include phone{
            .breadcrumb-list{display: none;}
            // .mob-breadcrumb{@include dispflex(flex,center,center); max-width: 35px; min-height: 35px; background: $whiteColor; box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.15); border-radius: 50%; margin-bottom: 20px;
            //     .breadscrumb-link{margin-bottom: 3px}
            // } 
            .mob-breadcrumb{margin-bottom: 20px; display: block;
                .breadscrumb-link{@include dispflex(flex,flex-start,center); color: $primarycolor; font-family: $Helvetica-500; font-size: 14px;
                    .icon-box{margin-right: 10px;}
                }
            } 
        }
}

//Page Controls
.page-controls{ @include dispflex(flex,space-between,center);
    .page-control-item{@include dispflex(flex,flex-start,center);cursor: pointer;
        .page-control-link{font-family: $Helvetica-500;}
        &.prev-btn{
            &::before{content: ''; display: inline-block; background: url(../images/icons/prev-page-arrow.png) no-repeat center center; width: 10px; height: 26px; margin-right: 10px;}
        }
        &.next-btn{
            &::after{content: ''; display: inline-block; background: url(../images/icons/next-page-arrow.png) no-repeat center center; width: 10px; height: 26px; margin-left: 10px;}
        }
        &:HOVER{
            .page-control-link{color: $primarycolor;}
        }
    }
}

//Pagination
.pagination-wrapper{ padding-top: 30px;
    .pagination-inner{
        .pagination-inner-parent{@include dispflex(flex,center,center);       
            .pagination-item{cursor: pointer; flex: 0 0 30px; max-width: 30px; border-radius: 5px; min-height: 30px; margin-right: 10px; transition: $transitionCommon;
                a{color: $lightTextColor; font-family: $Helvetica-500; text-align: center; font-size: 18px;}
                &:hover{
                    background: $primarycolor;
                }
                &:hover a{ color: $whiteColor;}
                &.active{  background: $primarycolor;
                    a{color: $whiteColor;}
                }
            }
            .pagination-next,.pagination-prev{min-height: 26px;
                .next,.prev{border: none; background: transparent; cursor: pointer;
                  &:focus{outline: none;}
                }
            }
            .pagination-prev{padding-right: 30px;}
            .pagination-next{padding-left: 30px;}
        }
    }
    @include phone-6{
        .pagination-inner .pagination-inner-parent{
            .pagination-item{margin-right: 8px;}
            .pagination-prev{padding-right: 20px;}
            .pagination-next{padding-left: 20px;}
        }
    }
}

.slider-wrap{
    .slider-col{
        .slider-box{
            .slider-area{margin: 0 -15px;}
            .slider-inner{padding: 0px 15px;
                .slider-content-box{position: relative;}
            }
        }
    }
}

.js-videos{margin: 0 -5px;
    .slider-inner{ padding: 0 5px;
        .slider-content-box{
            .tag-box{top: 10px; right: 10px; border-radius: 5px; padding: 1px 9px;
                .tag-text{font-size: 13px;}
            }
            .video-box{border-radius: 20px; overflow: hidden; position: relative;
                display: flex; flex-direction: row; box-shadow: 7px 7px 20px rgba(65,91,122,0.15);
                .video-player{width: 100%;}
                .play-button{opacity: 0; transition: $transitionCommon; position: absolute; top: 0; left: 0; right: 0; bottom: 0; @include dispflex(flex,center,center);width: 50px; height: 50px; border-radius: 50%; margin: auto; z-index: 2;}
                .description-box{position: absolute; bottom: 10px; left: 10px; z-index: 2;
                    .description-text{color: $whiteColor; font-family: $Helvetica-500;}
                }
                &::after{content: ''; position: absolute; bottom: 0; left: 0; height: 70%; width: 100%; background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, #000000 100%);}
            }
        } 
        &.slick-current{
            .slider-content-box .video-box{
                .play-button{opacity: 1;}
                // .video-player .video-element{height: 190px;}
            } 
        }
    }
}

.video-content-box{
    .tag-box{top: 10px; right: 10px; border-radius: 5px; padding: 1px 9px;
        .tag-text{font-size: 13px;}
    }
    .video-box{border-radius: 20px; overflow: hidden; position: relative;
        display: flex; flex-direction: row; box-shadow: 7px 7px 20px rgba(65,91,122,0.15);
        .video-player{width: 100%;
            .video-element{width: 100%; height: 170px; object-fit: cover; transition: $transitionCommon;}
        }
        .play-button{transition: $transitionCommon; position: absolute; top: 0; left: 0; right: 0; bottom: 0; @include dispflex(flex,center,center);width: 50px; height: 50px; border-radius: 50%; margin: auto; z-index: 2;}
        .description-box{position: absolute; bottom: 10px; left: 10px; z-index: 2;
            .description-text{color: $whiteColor; font-family: $Helvetica-500;}
        }
        &::after{content: ''; position: absolute; bottom: 0; left: 0; height: 70%; width: 100%; background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, #000000 100%);}
    }
}

.bg-white-box{ background: $whiteColor;border: 1px solid #DDD;border-radius: 15px; cursor: pointer; transition: $transitionCommon;
    &:hover{box-shadow: 0px 0px 60px rgba(0, 0, 0, 0.15); border-color:transparent;}
}
.tag-box{position: absolute; padding: 0 5px; text-align: center; z-index: 2; color: $whiteColor;
    .tag-text{font-family: $Helvetica-500;}
}
.bg-white-wrapper-box{background: $whiteColor; box-shadow: 10px 10px 40px rgba(26, 129, 255, 0.1);border-radius: 20px;}

//Blog section common box
.blog-main-box{position: relative; max-width: 350px;
    .blog-image-area{position: absolute; top: -40px; left: 15px;
        .blog-image-box{position: relative; border-radius: 20px; overflow: hidden;
            img{height: 220px; object-fit: cover;}
            .tag-box{top: 18px; left: 0; background: $blackTransparent; color: $whiteColor; border-radius: 0px 8px 8px 0px; padding: 5px; min-width: 140px;
            }
        }
    }
    .blog-detail-box{min-height: 350px; @include dispflex(flex,flex-start,flex-end);
        .blog-body{padding: 15px;
            .blog-title{margin-bottom: 8px; font-family: $Helvetica-700; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden;}
            .blog-description{margin-bottom: 10px; display: -webkit-box; -webkit-line-clamp: 3;
                -webkit-box-orient: vertical; overflow: hidden;}
            .blog-footer{justify-content: space-between;
                .read-more-btn{ font-family: $Helvetica-500;
                    .btn-text{margin-right: 5px; font-family: $Helvetica-500;}
                }
            }
        }
    }
    .blog-bg-box{background: $whiteColor; box-shadow: 10px 10px 40px rgba(26, 129, 255, 0.15);
    border-radius: 20px;}
    
    @include big-desktop{max-width: 100%;
        .blog-image-area{right: 15px;
            .blog-image-box{
                img{width: 100%;}
            }
        }
    }
    @include mini-desktop{
        .blog-image-area { left: 8px; 
            .blog-image-box img{height: 190px;}
            &.mob-change{left: 0px; padding: 10px;
                .blog-image-box{
                    img{height: 100%;}
                }
            }
        }
    }
    @include tab{
        .blog-image-area { 
            .blog-image-box{
                img{height: 135px;}
                .tag-box{min-width: 120px;
                    .tag-text{font-size: 14px;}  
                } 
            } 
        }
        .blog-detail-box{min-height: 290px;
            .blog-body .blog-footer{
                .small-detail-text{font-size: 13px;}
                .read-more-btn{
                    .btn-text{font-size: 14px;}
                    .btn-icon img{height: 8px;}
                } 
            } 
        } 
    }
    @include phone{
        @include dispflex(flex,flex-start,center); max-width: 100%;
        .blog-image-area{position: static; flex: 0 0 30%; max-width: 30%;
            .blog-image-box{ border: 10px; border-top-right-radius: 0px; border-bottom-right-radius: 0px;
                img{height: 165px; width: 100%;}
                .tag-box{display: none;} 
            }
            &.mob-change{padding:0px;
                .blog-image-box{
                    img{height: 165px;}
                }
            }
        }
        .blog-detail-box{display: block; min-height: auto; flex: 0 0 70%; max-width: 70%; min-height: 165px; @include dispflex(flex,center,center); flex-direction: row;
            &.blog-bg-box{border: 10px; border-top-left-radius: 0px; border-bottom-left-radius: 0px;}
            .blog-body .blog-footer .read-more-btn .btn-icon img{height: 10px;}
        }
    }
    @include phone-xs{
        .blog-image-area{
            .blog-image-box img{height: 135px;}
            &.mob-change{padding:0px;
                .blog-image-box{
                    img{height: 135px;}
                }
            }
        } 
        .blog-detail-box{min-height: 135px;
            .blog-body{
                .blog-title{margin-bottom: 5px; -webkit-line-clamp: 1;}
                .blog-description{margin-bottom: 5px; display: -webkit-box; -webkit-line-clamp: 2;
                -webkit-box-orient: vertical; overflow: hidden; text-overflow: ellipsis;}
            } 
        } 
    }
    @include phone-6{
        .blog-image-area{
            .blog-image-box img{height: 125px;}
            &.mob-change{
                .blog-image-box{
                    img{height: 125px;}
                }
            }
        }
