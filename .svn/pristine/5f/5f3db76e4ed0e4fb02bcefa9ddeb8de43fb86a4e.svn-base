		function Verifying_quote(){
			$('.okBtn').hide();
			$('.VehEligiblePopUp_Text').show();
			var quoteInSecond = 5
			$('.quoteCountDown').html(quoteInSecond);
			myVar = setInterval(function(){
				quoteInSecond--;
				$('.quoteCountDown').html(quoteInSecond);
				//console.log(quoteInSecond);
				if(quoteInSecond == 0){
					clearInterval(myVar);
					$('.VehEligiblePopUp_Text').hide();
					$('.okBtn').show();
				}
			}, 1000);
		}
