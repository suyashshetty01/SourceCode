// Connect Us style
.connect-us-section{
    .connect-us-form-section{
        // .sub-title{margin-bottom: 30px;}
        .form-field-area{
            .form-field-box{
                .form-field-row{
                    .form-field-col{margin-bottom: 15px;
                        &:first-child{margin-bottom: 40px;}
                        .flex-box{ margin: 0 -7px; align-items: flex-start;
                            .flex-50{flex: 0 0 50%; max-width: 50%; padding: 0 7px;}
                        }
                    }
                } 
            }
        }
        .radio-block{ border: 2px solid #DDD; box-sizing: border-box; border-radius: 15px; text-align: center; color: #777; padding: 8px 0; display: flex; justify-content: center; align-items: center;
            transition: $transitionCommon; font-weight: 700;
            .icon-box img{transition: $transitionCommon;}
            &:hover{ color: $primarycolor; border-color: $primarycolor;
                .icon-box img{filter: invert(34%) sepia(40%) saturate(1132%) hue-rotate(191deg) brightness(90%) contrast(110%);}
            }
            &.active{ background: #393185; color: $primarycolor; } 
            .icon-box{margin-right: 8px; padding-bottom: 3px;}
        }
        .radio-custom + .radio-block::before{ position: absolute; opacity: 0; }
        .radio-custom:checked + .radio-block { border-color: $primarycolor; color: $primarycolor; 
            .icon-box img{filter: invert(34%) sepia(40%) saturate(1132%) hue-rotate(191deg) brightness(90%) contrast(110%);}
        }
    }   
    .image-box{
        img{height: 450px; object-fit: cover;}
    }
