.tab-change-wrap{
    .tab-change-btn{border: none; color: #8D939A; font-family: $Helvetica-500; transition: $transitionCommon; border: 0px; border-bottom: 2px solid transparent; padding: 13px 13px; position: relative; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; border-bottom: 0px;
        &.flex-col-4{flex: 0 0 25%; max-width: 25%;}
        &.flex-col-3{flex: 0 0 33.33%; max-width: 33.33%;}
        &.active{color: $primarycolor; border-bottom: 2px solid $primarycolor; border-color: $primarycolor; border-bottom: 0px;
            &:hover{border-color: $primarycolor; border-bottom: 0px;}
            &:focus{border-color: $primarycolor; border-bottom: 0px;}
            &::before{opacity: 1;}
        }
        &::after{content: ''; position: absolute; top: 0; right: 0; width: 1px; height: 100%; background-color: rgba(0, 0, 0, 0.1);}
        &::before{content: ''; position: absolute; bottom: 0; right: 0; width: 100%; height: 2px; background-color: $primarycolor; opacity: 0; transition: $transitionCommon;}
        &:last-child{border-top-left-radius: 0;border-top-right-radius: 12px;
            &::after{display: none;}
        }
        .tab-text{font-size: 0px;}
        &:hover{border-color: transparent;}
        &:focus{border-color: transparent;}
        &.visited{color: $primarycolor;}
    }
    &.tab-view-main{
        .tab-change-btn{
            &::after{display: none;}
        }
    }
    @include phone{justify-content: center;
        .tab-change-btn{ border-bottom: 0px; padding:13px 20px; 
            margin-right: 30px; font-size: 0px; overflow: visible;
            &.flex-col-4{flex: 0 0 auto; max-width: 100%;}
            &.flex-col-3{flex: 0 0 auto; max-width: 100%;}
            &::after{ top: 27px; right: -42px; width: 55px; height: 2px; transition: $transitionCommon; z-index: 5;}
            &::before{display: none;}
            &.active{border-bottom: 0px;
                &::after{ background-color: $primarycolor;}
            }
            .tab-text{ display: block; width: 30px; height: 30px; margin:0 auto; border: 2px solid;
            border-radius: 50%; font-size: 16px;}
            &:last-child{padding-right: 0px;}
            &:first-child{padding-left: 0px;}
            .tab-text{transition: $transitionCommon;}
            &.visited{
                .tab-text{background: $primarycolor; color: $whiteColor;}
                &::after{background: $primarycolor;}
            }
        }
    }
    @include phone-6{
        .tab-change-btn{padding: 13px 13px; margin-right: 20px;
            &::after{right: -30px; width: 40px;}
        }
    }
}

