        <script>
            $.fn.serializeObject = function () {
                var o = {};
                var a = this.serializeArray();
                $.each(a, function () {
                    if (o[this.name]) {
                        if (!o[this.name].push) {
                            o[this.name] = [o[this.name]];
                        }
                        o[this.name].push(this.value || '');
                    } else {
                        o[this.name] = this.value || '';
                    }
                });
                return o;
            };

            $(function () {
                $('#member_1_dob').bootstrapMaterialDatePicker({
                    time: false, clearButton: true, format: 'DD-MM-YYYY',
                    //minDate: moment(),
                    maxDate: moment().subtract(18, 'years'), // Current day
                    defaultDate: moment().subtract(30, 'years'),
                    onSelect: function () {
                        myfunction();
                    }
                }).on('change', function (e, date) {
                    $(".MyTable").show();
                    var arr = $("#member_1_dob").val().split('-');
                    var Days = (((new Date(Date.now())).getTime()) - ((new Date(arr[1] + "-" + arr[0] + "-" + arr[2])).getTime())) / (1000 * 60 * 60 * 24);
                    var age = (Days) / 365;
                    $('[name="plan_option"]').removeAttr('checked');
                    if (age < 45) {
                        $(".U45").show();
                        $(".A45").hide();
                        $("input[name=plan_option][value=1M_45U]").attr('checked', 'checked');
                    } else {
                        $(".U45").hide();
                        $(".A45").show();
                        $("input[name=plan_option][value=1M_45A]").attr('checked', 'checked');
                    }
                    OpenSection();
                });

                $('#member_2_dob,#member_3_dob,#member_4_dob,#member_5_dob,#member_6_dob').bootstrapMaterialDatePicker
                ({                            
                    clearButton: true,
                    time: false,
                    maxDate: new Date()
                });
                $('#btnContinue').on('click', function (e) {
                    e.preventDefault();
                    var odata = {};
                    var formData = $('#frmnsci').serializeObject();
                    var is_valid = false;
                    for (var k in formData) {
                        if ($("#" + k).is(":visible") && formData[k] == "") {
                            is_valid = false;
                            alert("Please enter " + k);
                            break;
                        } else {
                            is_valid = true;
                            odata[k] = formData[k];
                        }
                    }

                    console.log(formData);

                    if (is_valid == true) {
                        $.ajax({
                            type: 'POST',
                            url: 'http://horizon.policyboss.com:5000/report/nsci_lead_start',
                            data: formData,
                            dataType: "json",
                            success: function (data) {
                                if (data.status == "success") {
                                    $('#myModal').modal('show');
                                    $("#ack").val(data.ack);
                                }
                            },
                            error: function (data) {
                                alert("Cannot Proceed Now. Please Try Again!");
                            }
                        });
                    }
                    return false;
                });
                $('#frmnsci').on('submit', function (e) {
                    e.preventDefault();
                    var odata = {};
                    var formData = $('#frmnsci').serializeObject();
                    for (var k in formData) {
                        if ($("#" + k).is(":visible") && formData[k] == "") {
                            alert("Please enter " + k);
                            return false;
                        } else {
                            odata[k] = formData[k];
                        }
                    }
                    console.log(formData);

                    if ($("#otp_entered").val() !== '') {
                        $.ajax({
                            type: 'POST',
                            url: 'http://horizon.policyboss.com:5000/report/nsci_lead_save',
                            data: formData,
                            dataType: "json",
                            success: function (data) {
                                if (data.status == "success") {
                                    $('#myModal').modal('hide');
                                    alert('Thank you for enrollment. Acknowledge Number - ' + data.ack);
                                } else {
                                    alert("Please entered correct OTP");
                                }
                            },
                            error: function (data) {
                                alert("Cannot Proceed Now. Please Try Again!");
                            }
                        });
                    } else {
                        alert("Please enter OTP");
                    }
                    return false;
                });
            });
        </script>
    </body>
</html>