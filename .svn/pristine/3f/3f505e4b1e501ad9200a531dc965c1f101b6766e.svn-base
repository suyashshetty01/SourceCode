.select-tab-area{display: block;}
.select-tab-item{
    .select-tab-box{border: 2px solid rgba(0,0,0,0.1); border-radius: 12px; padding: 0px 5px; min-height: 130px; @include dispflex(flex,center,center); flex-direction: row; width: 100%; transition: $transitionCommon;
        &.active{background-color: $whiteColor; border-color: $primarycolor;
            .radio-name-text{color: $primarycolor;}
        }
        .image-box{margin-bottom: 15px;
            img{margin: 0 auto; height: 35px;}
        } 
        &.tab-box-horizontal{min-height: 55px; position: relative;
            .inner-box{@include dispflex(flex,center,center); flex-direction: row; width: 100%;
                .image-box{margin-bottom: 0px; margin-right: 10px;
                    img{height: 25px;}
                }
            }
            .bottom-label-box{position: absolute; bottom: -20px; left: 0; right: 0; padding: 2px 0px; margin: 0 auto;
            font-size: 9px; font-family: $Helvetica-500; background: rgba(25, 129, 255, 0.1); border-radius: 0px 0px 12px 12px; text-align: center; max-width: 105px;}
        }
    }
    @include phone-xr{
        .select-tab-box.tab-box-horizontal .inner-box .image-box img{height: 20px;}
    }
    @include phone-6{
        .select-tab-box{min-height: 100px;
            .image-box{margin-bottom: 5px;
                img{height:25px;}
            } 
        }
    }
    @include phone-6{
        .select-tab-box{min-height: 90px;
            .image-box img{height:20px;}
        }
    }
}

.help-block{color: $errorColor;
    @include phone-xs{font-size: 14px;}
}
.has-error{border: 1px solid $errorColor;
    &:focus{border: 1px solid $errorColor;}
    &.border-thick{border: 2px solid $errorColor;
        &:focus{border: 2px solid $errorColor;}
    }
}

.container-box{max-width: 800px; margin: 0 auto;
    @include mini-desktop{max-width: 700px;}
    @include tab{max-width: 580px;}
    @include phone{}
    @include phone-xs{}
    @include phone-6{}
}

.inner-placeholder{
    &.input-change{padding-top: 14px; font-size: 18px; color: #444;}
    &::placeholder{transition: $transitionCommon;}
    &:focus{padding-top: 14px; font-size: 18px;
        &::placeholder{position: absolute; top: 3px; left: 20px; font-size: 14px; }
    }
    @include phone-6{
        &.input-change{padding-top: 15px; font-size: 15px; }
    }
}

.floating-placeholder{color: $lightGray; font-family: $Helvetica-500; position: absolute; top: 16px; left: 22px; opacity: 1; transition: 0.2s ease-in-out; pointer-events: none;
    @include phone-xs{left: 12px;}
    @include phone-6{top: 10px;}
}
.input-field:focus ~ .floating-placeholder{transform: translateY(-10px); font-size: 14px;}
.input-change ~ .floating-placeholder{transform: translateY(-10px); font-size: 14px; top: 18px;}
@include phone-6{
    .input-field:focus ~ .floating-placeholder{transform: translateY(-5px);top: 5px;}
    .input-change ~ .floating-placeholder{transform: translateY(-5px); font-size: 13px; top: 10px;}
}


input::placeholder{color: $lightGray; font-family: $Helvetica-500;}
input[type="number"] {
    -moz-appearance: textfield;
}
.common-dropdown{color: $lightGray;font-family: $Helvetica-500; appearance: none; position: relative;
    background-image: url(../images/icons/dropdown-arrow-light.png); background-repeat: no-repeat; background-position: calc(100% - 20px) center;
    &.dropdown-open{background-image: url(../images/icons/dropdown-arrow-light-up.png); background-repeat: no-repeat; background-position: calc(100% - 20px) center;}
}
select:invalid {
    color: $lightGray;
}

//Bootstrap Datepicker Style Override
.datepicker td{padding: 5px;}
.datepicker table tr td.today{background: #003ACC; color: $whiteColor;}
.datepicker table tr td.active:active, 
.datepicker table tr td.active.highlighted:active, 
.datepicker table tr td.active.active, 
.datepicker table tr td.active.highlighted.active {
  background-color: $primarycolor;
  background-image: none;
}
.datepicker table tr td.active:hover:hover{background-color: $primarycolor;}
.datepicker table tr td.day.focused, .datepicker table tr td.day:hover{background-color: #80b1ed; color: $whiteColor;}
.datepicker table tr td.disabled, .datepicker table tr td.disabled:hover{cursor: not-allowed; background-color: $whiteColor; color: #999;}
.bdpw-clock-digit.cursor{color: $primarycolor;}

//Modal with different sizes
.wrapper-540{max-width: 540px; margin: 0 auto;
    @include phone{max-width: 100%; margin: 0px;}
}
.wrapper-945{max-width: 945px; margin: 0 auto;}
.wrapper-750{max-width: 750px; margin: 0 auto;}
.modal-popup-shadow{background: $whiteColor; box-shadow: 0px 5px 25px rgba(0, 0, 0, 0.25); border-radius: 10px;
    @include phone{border-radius: 0px; box-shadow:none;}
}

//Radio button custome style
.radio-custom { opacity: 0; position: absolute; }
.radio-custom, .radio-custom-label { display: inline-block; vertical-align: middle; cursor: pointer; }
.radio-custom-label { position: relative; transition: $transitionCommon;}
.radio-custom + .radio-custom-label:before { content: ''; background: #fff; border: 2px solid rgba(0, 0, 0, 0.1); display: inline-block; vertical-align: middle; width: 24px; height: 24px; padding: 2px; margin-right: 6px; text-align: center; }
.radio-custom + .radio-custom-label:before { border-radius: 50%; }
.radio-custom:checked + .radio-custom-label:before { background: $whiteColor; box-shadow: inset 0px 0px 0px 3px #fff; border: 6px solid $primarycolor; }

//Radio button with image custome style
.radio-image:checked + .radio-custom-label{ border: 2px solid #1981FF; box-shadow: 10px 10px 40px rgba(26, 129, 255, 0.1);
    .radio-name-text{color: $primarycolor;}
}
.radio-image-label{text-align: center; padding: 8px 5px; transition: $transitionCommon; min-height: 130px; border-radius: 12px; @include dispflex(flex,center,center); flex-direction: column; 
    &.radio-image-label-small{min-height: 55px;}
    .icon-box{height: 75px; width: 75px; border-radius: 50%; overflow: hidden; margin: 0 auto; margin-bottom: 8px;
        img{height: 75px;}
    }
    .image-box{text-align: center;}
    .inner-box{@include dispflex(flex,center,center); flex-direction: row; 
        .image-box{margin-right: 10px;}
    }
    @include phone-xr{min-height: 120px;
        .icon-box{height: 65px; width:65px;
            img{height: 65px;}
        }
    }
    @include phone-6{min-height: 105px;
        .icon-box{height: 50px; width:50px;
            img{height: 50px;}
        }
    }
    @include phone-5s{min-height: 90px; padding: 10px 3px;
        .icon-box{height: 45px; width:45px; margin-bottom: 5px;
            img{height: 45px;}
        }
    }
} 
.radio-input-hidden{
    &::before{opacity: 0; position:absolute; }
}
.radio-name-text{font-family: $Helvetica-500; transition: $transitionCommon;
    @include phone-6{font-size: 14px;}
    @include phone-5s{font-size: 12px;}
}
.label-flex{@include dispflex(flex,center,center); flex-direction: row;}
.ui-autocomplete{z-index: 9999;}

//Checkbox custome style
.check-box-select{
    
}
.check-input { padding: 0; height: initial; width: initial; margin-bottom: 0; display: none;cursor: pointer;}
.check-input-label {position: relative;cursor: pointer;}
.check-input-label::before { content:''; -webkit-appearance: none; background: $whiteColor; border: 2px solid rgba(0, 0, 0, 0.1); box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05); padding: 8px; display: inline-block; position: relative; vertical-align: middle; cursor: pointer; border-radius: 5px; transition: $transitionCommon;}
.check-input:checked + .check-input-label::before{background: $primarycolor; border-color: transparent;}
.check-input:checked + .check-input-label::after { content: ''; display: block; position: absolute; top: 6px; left: 7px; width: 6px; height: 12px; border: solid $whiteColor; border-width: 0 2px 2px 0; transform: rotate(45deg);}


.checkbox-area{
    ::-webkit-scrollbar { width: 5px; height: 5px; }
    ::-webkit-scrollbar-track { box-shadow: inset 0 0 5px white; }
    ::-webkit-scrollbar-thumb { background: $primarycolor; }
}
