        <script type="text/javascript">
            function OpenSection() {
                for (var k = 2; k <= 6; k++) {
                    $('#member_' + k + '_section').hide();
                }
                var val = $("input[name='plan_option']:checked").val();
                var member_cnt = val.split('M_')[0];
                if (member_cnt > 1) {
                    for (var k = 2; k <= member_cnt; k++) {
                        $('#member_' + k + '_section').show();
                    }
                }
                SetPremium();
            }
            $("#sum_insured").change(function () {
                SetPremium();
            });

            function SetPremium() {
                var val1 = $("input[name='plan_option']:checked").val();
                var val2 = GetIndex();
                var PremiumText = $("#" + val1 + "_" + val2).text();
                //alert($(PremiumText).replace(",",""));
                $("#premium").val(PremiumText.replace(/,/g, ''));
                alert($("#premium").val());
            }
            function GetIndex() {
                var Val = $("#sum_insured").val();
                var Index;
                switch (Val) {
                    case "500000": Index = 1; break;
                    case "1000000": Index = 2; break;
                    case "1500000": Index = 3; break;
                    case "2000000": Index = 4; break;
                    case "2500000": Index = 5; break;
                    case "3000000": Index = 6; break;
                }
                return Index;
            }

            function Validate() {
                if ($("#mobile_no").val().length != 10) {
                    alert("Enter Valid Mobile");
                    return false;
                }
            }

            $(document).ready(function () {
                $(".U45").hide();
                $(".A45").hide();
                $(".MyTable").hide();
            });

        </script>
