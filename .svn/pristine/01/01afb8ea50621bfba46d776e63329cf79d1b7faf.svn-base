        .location-section-box{
            .location-zone-title{justify-content: flex-start; padding: 0px; margin-bottom: 20px;
                &::before{display: none;}
                &::after{display: block; background: url(../images/icons/accordion-dark-arrow.png) no-repeat center center;}
            }
            .location-listing-area{padding-right: 30px;
                .listing-ul{margin: 0px;
                    .listing-li{@include flex_box(1); padding: 0px; margin-bottom: 15px;}
                }
                // &:last-child .location-zone-title{margin-bottom: 0px;}
            } 
        }
    }
    @include phone-xs{padding-bottom: 40px;}
}
//Customer Service Section
