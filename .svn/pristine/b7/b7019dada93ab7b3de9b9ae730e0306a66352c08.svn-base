        "Title": req.body['title'],
        "URL": req.body['URL']
    };
    if (req.body['data_val'] === "save") {
        objData['Status'] = "Preview";
        objData['Draft_Data'] = obj;
    } else if (req.body['data_val'] === "edit_save") {
        objData['Draft_Data'] = obj;
        objData['Status'] = "Preview";
    }/*else{
     objData['Draft_Data'] = obj;
     objData['Final_Data'] = obj;
     }*/
