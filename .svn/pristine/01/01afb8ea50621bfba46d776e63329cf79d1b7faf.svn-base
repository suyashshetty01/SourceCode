                .claims-listing-box{margin: 0px;
                    .flex-col-3{@include flex_box(2); margin-bottom: 20px;
                        &.align-self-center{align-self: flex-start !important;}
                    }
                }
                .contact-info-row .contact-info-box{margin: 0px; flex-wrap: wrap; padding: 0 10px;
                    .flex-col-4{padding: 0px; @include flex_box(1); margin-bottom: 10px;
                        &:last-child{margin-bottom: 0px;}
                    }
                    .contact-box .icon-box{flex: 0 0 25px; max-width: 25px;}
                } 
            } 
        } 
    }
    @include phone{}
    @include phone-xs{
        .claims-main-area{
            .claims-listing-area .claims-listing-row{
                .claims-listing-box{
                    .heading-text{display: none;}
                    .content-box{ @include dispflex(flex,flex-start,self-start); flex-direction:row;
                        &::before{content: ''; display: inline-block; width: 20px; height: 20px; flex: 0 0 20px; max-width: 20px; margin-right: 12px;}
                        &.insurance-title-box{
                            &::before{background: url(../images/icons/insurance-icon.png) no-repeat center center;}
                        }
                        &.address-box{
                            &::before{background: url(../images/icons/location-light.png) no-repeat center center;}
                        }
                        &.contact-box{
                            &::before{background: url(../images/icons/phone-icon-light.png) no-repeat center center;}
                        }
                    }
                    .flex-col-3{@include flex_box(1); margin-bottom: 15px; padding: 0px;
                        &:last-child{margin-bottom: 0px;}
                    }
                }
                .contact-info-row .contact-info-box{padding: 0px;} 
            } 
        }
    }
    @include phone-6{
        .claims-main-area{
            .search-box .input-field{font-size: 14px;}
            .claims-listing-area .claims-listing-row .contact-info-row{
                .contact-info-box{
                    .flex-col-4{align-items: baseline;}
                    .contact-box .icon-box{ flex: 0 0 20px; max-width: 20px;
                        .mail-icon{height: 16px;}
                    }
                    .description-text{word-break:break-word;}
                } 
            } 
        }    
    }
} 

