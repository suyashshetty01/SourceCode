    @include phone-5s{
        .contact-location-section .location-area .location-image-area .location-image-box{
            img{height: 150px;}
        } 
    }
    @include landscape-mode {
        .contact-form-area .contact-form-box .contact-form .contact-form-inner .input-field-box { flex: 0 0 100%; max-width: 100%;
