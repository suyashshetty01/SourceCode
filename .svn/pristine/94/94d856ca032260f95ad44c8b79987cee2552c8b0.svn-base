        function SubmitForm() {
            var error_cnt = 0;

            if ($('#txt_Comment').val() === null || $('#txt_Comment').val() === "") {
                error_cnt = 1;
                $('#txt_Comment').addClass('Error');
            }
            else {
                $('#txt_Comment').removeClass('Error');
            }
			 if ($('#Ticket_Status').val() === "0") {
                error_cnt = 1;
                $('#Ticket_Status').addClass('Error');
            }
            else {
                $('#Ticket_Status').removeClass('Error');
            }

