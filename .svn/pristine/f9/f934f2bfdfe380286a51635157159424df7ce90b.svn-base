@keyframes my-animation {
  from {
    opacity : 0;
    left : -500px;
  }
  to {
    opacity : 1;
    left : 0;
  }
}

.run-animation {
  position: relative;
  animation: my-animation 2s ease;
}


@keyframes my-box {
  from {
    opacity : 0;
    right : -500px;
  }
  to {
    opacity : 1;
    right : 0;
  }
}

.run-frame {
  position: relative;
  animation: my-box 2s ease;
}

