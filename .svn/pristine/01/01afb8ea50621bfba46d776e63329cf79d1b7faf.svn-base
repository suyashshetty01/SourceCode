            }
            .btn-close-wrapper{ flex: 0 0 30%; max-width: 30%; text-align: right; padding: 25px;
                padding-bottom: 0px; height: 100%; align-self: stretch; background: #F4F3F3; min-height: 61px;
                .btn-close{padding: 0px; margin: 0px;}
            }
        }   
        .modal-body{ padding: 0px;
            .modal-content-area{@include dispflex(flex,flex-start,flex-start); flex-direction: row;
                .policy-detail-area{flex: 0 0 70%; max-width: 70%; padding: 25px;
                    .policy-detail-box{
                        .flex-box{justify-content: space-between;}
                        .title-text{margin-bottom: 30px;}
                        .premium-breakup-area{
                            .premium-breakup-box{margin-bottom: 25px;
                                .premium-sub-title{color: $lightTextColor; text-align: left; margin-bottom: 0px;}
                                &:last-child{margin-bottom:0px ;
                                    &::after{content: ''; display: block; height: 1px; width: 100%; background: #ddd; margin: 25px 0px;}
                                }
                            } 
                        }
                        .premium-detail-box{@include dispflex(flex,space-between,center);
                            .premium-title-box{
                                .premium-title{font-family: $Helvetica-500;}
                                .premium-value{font-family: $Helvetica-500;}
                            }
                        }
                        .policy-coverage-area{margin-bottom: 40px;}
                        .claims-coverage-area{
                            .claims-coverage-listing{
                                .claims-coverage-box{ @include dispflex(flex,space-between,center); flex-direction: row; min-height: 60px; padding: 0 20px; border-radius: 10px; margin-bottom: 20px;
                                    .claims-coverage-text-box{
                                        .claims-coverage-text{font-size: 14px; color: #666; max-width: 470px;}
                                    }
                                }
                            } 
                        }
                        .row-area{@include dispflex(flex,flex-start,center); flex-direction: row; flex-wrap: wrap; margin: 0 -10px;
                            .col-area{@include flex_box(3); padding: 0 10px; margin-bottom: 20px;
                                .box-area{@include dispflex(flex,flex-start,center); flex-direction: row; padding: 0px; min-height: 50px; border-radius: 5px; background: $whiteColor; box-shadow: (0px 4px 10px rgba(0, 0, 0, 0.1));
                                    .icon-box{flex: 0 0 50px; max-width: 50px; margin-right: 10px; height: 50px; background: #F4F9FF; border-radius: 5px 0px 0px 5px;
                                    display: flex; justify-content: center; align-items: center;}
                                    .text{font-size: 14px; color: #666;} 
                                }
                                &:last-child{margin-bottom: 0px;}
                            }
                        }
                        .cashless-garage-area{
                            .cashless-garage-listing{@include dispflex(flex,flex-start,flex-start); flex-direction: row; flex-wrap: wrap; margin: 0 -10px; overflow: auto;
                                max-height: 500px; padding-top: 20px;
                                .cashless-garage-col{@include flex_box(2); padding: 0 10px; margin-bottom: 20px;
                                    .cashless-garage-box{box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.1); border-radius: 10px; padding: 20px;
                                        .garage-title{margin-bottom: 0px;}
                                        .link-box{@include dispflex(flex,flex-start,flex-start); flex-direction: row; color: #777;
                                            .icon-box{margin-right: 10px;}
                                        }
                                        .call-link{margin-bottom: 8px;}
                                    }
                                }
                            }
                        }
                    }
                }
                .policy-info-area{flex: 0 0 30%; max-width: 30%; background: $bgOffWhite; padding: 0 25px 25px 25px; align-self: normal;
                    .policy-info-box{
                        .insurance-logo-box{text-align: center; background: #FFFFFF; border-radius: 10px 12px 12px 10px; padding: 11px; margin-bottom: 20px;
                            img{}
                        }
                        .policy-info-detail-box{
                            .poilcy-info-title{margin-bottom: 20px;}
                            .policy-info-listing{@include dispflex(flex,flex-start,center); flex-wrap: wrap;
                                .policy-info-listing-col{flex: 0 0 50%; max-width: 50%; margin-bottom: 25px;
                                    &:first-child{flex: 0 0 100%; max-width:100%;}
                                    .policy-info-listing-box{
                                        .policy-info-listing-title{color: $lightTextColor;}
                                        .policy-info-listing-text{font-family: $Helvetica-500;}
                                    }   
                                }
                            }
                        }
                    }
                }
            }
        }   
}

//Insurance Summary
.insurance-summary-modal{}     
.insurance-summary-area{
    .profile-detail-text{font-size: 16px; margin-bottom: 10px;}
    .insurance-summary-sub-title{margin-bottom: 20px;}
    .insurance-summary-box{
        .insurance-summary-listing{border: 1px solid #ccc; border-radius: 10px;
            .insurance-summary-listing-row{@include dispflex(flex,flex-start,stretch);
                .flex-65{flex: 0 0 65%; max-width: 65%;} 
                .flex-35{flex: 0 0 35%; max-width: 35%;} 
                .listing-box{padding: 5px 10px; border-bottom: 1px solid #ccc; border-right: 1px solid #ccc;
                    &:last-child{border-right: 0px;}
                }
                .listing-heading{
                    h6{margin-bottom: 0px; font-family: $Helvetica-700;}
                }
                &:last-child{
                    .listing-box{border-bottom: 0px;}
                }
            }
        }
    }
}

.select-dropdown{display: block; text-align: left; background-image: url(../images/icons/dropdown-arrow-light.png); background-repeat: no-repeat; background-position:calc(100% - 20px) center;
    &:focus{box-shadow: none;}
    &::placeholder{color: #222 !important;}
}
.select-dropdown-box{padding:0px;overflow: hidden; width: 100%;
    .select-dropdown-box-ul{
        .select-dropdown-box-li{
            .select-dropdown-box-link{padding: 10px;}
        }
    }
}


//Footer Section
.footer-section{      
    .footer-links{ margin-bottom: 30px;
        .footer-link-box{
            .footer-link-heading{margin-bottom: 10px; color: $whiteColor; font-family: $Helvetica-700;}
            ul li{margin-bottom: 5px;
                a{color: $whiteColor; font-family: $Helvetica-400; position: relative; display: inline-block;
                    &::after { background: none repeat scroll 0 0 transparent; bottom: 0; content: "";
                        display: inline-block; height: 1px; left: 0; position: absolute; background: $whiteColor; transition: width 0.4s ease 0s, left 0.4s ease 0s; width: 0;
                    }
                    &:hover{
                        &:after{  width: 100%; }
                    }
                }
            } 
        }
    }       
    .footer-social-links{margin-bottom: 40px;
        &::before{content: ''; display: block; width: 100%; min-height: 2px; background: rgba(255, 255, 255, 0.1); margin-bottom: 20px;}
        .socials-box{margin-bottom: 20px;
            ul{@include dispflex(flex, center, center); margin: 0 -8px;
                li{padding: 0 8px; transition:$transitionCommon;
                    &:hover{transform: translate(0px,-3px);}
                }
            }
        }
        &::after{content: ''; display: block; width: 100%; min-height: 2px; background: rgba(255, 255, 255, 0.1);}
    }
    .footer-other-details{text-align: center; color:#b2bbc5;
        .heading-text{margin-bottom: 12px;} 
        .box{
            .icon-box{margin-bottom: 20px;}
        }
        .payment-box{
            ul{@include dispflex(flex, flex-start, center); margin: 0 auto; background: $whiteColor; margin-bottom:30px; border-radius: 4px; overflow: hidden; max-width: 250px; 
                li{padding: 0 8px; flex: 0 0 60px; max-width: 60px; 
                    a{padding: 2px;
                        .icon-box{
                            img{height: 40px; width: auto;}
                            .visa-card{height: 25px;}
                        } 
                    }
                } 
            }  
        }
        .our-security{
            ul{background: $whiteColor; max-width: 270px; margin: 0 auto; margin-bottom: 30px; padding: 5px; border-radius: 4px;
                li{display: inline-block;
                    .box{ background: $whiteColor; padding: 5px; display: flex; align-items: center;
                        .icon-box{margin-bottom: 0px;}
                    } 
                } 
            } 
        } 
        .our-branch-section{max-width: 900px; margin: 0 auto; margin-bottom: 30px;
            .heading-text{margin-bottom: 5px;}
            .branch-title-row li{display: inline-block; font-size: 15px;
                &::after{content: '|'; display: inline-block; margin: 0 5px;}
                &:last-child{
                    &::after{display: none;}
                }
            }
        }
        .link-box{margin-bottom: 20px;
            a{display: inline-block; color: #b2bbc5;
                &::after{content: '|'; display: inline-block; margin: 0 5px;}
                &:last-child{
                    &::after{display: none;}
                }
            } 
        } 
        p{font-size: 15px;}
        .read-instruction-text{margin-bottom: 10px; font-size: 15px; color: $whiteColor; font-family: $Helvetica-500;}
        .address,.info-text{margin-bottom: 20px; font-size: 15px;}
    }
    .font-18{font-size: 16px;}
    .font-16{font-size: 14px;}
    @include phone{
        .footer-link-box{margin-bottom: 20px;}
    }
    
    @include phone-xs{
        .footer-links{margin-bottom: 20px;
            .footer-link-box{margin-bottom: 20px; position: relative; border-bottom: 2px solid rgba(255, 255, 255, 0.1);
                &::after{content: ''; background-image:url(../images/icons/accordion-arrow.png); background-repeat: no-repeat; background-size: auto; background-position: center; display: block; width: 30px; height: 30px;position: absolute; top: -3px; right: 0;}
                .footer-link-heading{font-family: $Helvetica-400; margin-bottom: 20px;}
                ul{max-height: 0px; height: auto; overflow: hidden; transition: max-height 0.5s ease-out;
                    li a{font-size: 14px;}
                } 
                &.active ul{max-height: 220px; height: auto; border-bottom: 0px; }
            }
        } 
        .footer-social-links{margin-bottom: 25px;
            &::before{display: none;}
            &::after{display: none;}
        }
        .footer-other-details{
            .read-instruction-text,.our-branch-section .branch-title-row li,.detail,.detail-description,.address,.link-box a,.info-text,.copyright-text{font-size: 13px;}
        } 
        .info-text{padding-bottom: 15px; border-bottom: 1px solid rgba(255, 255, 255, 0.1);}
    }           
}            
 
@-moz-document url-prefix() {
    .common-dropdown{padding: 15px 20px;}
}
     
.ui-timepicker-standard{display: none !important;}
.mdtp__wrapper{transform: translateX(-50%) translateY(-50%) scale(1); top: 50%;}
//main header
.mainHeader{
    &.not-fixed{position: relative;} 
}
.hamburger { position: absolute; display: block; width: 35px; height: 35px; cursor: pointer; appearance: none; outline: none; border: none; top: 12px; right: 0px; background: transparent;
    // @include phone { top: 5px; } 
    @include phone-xs { width: 30px; }
    @include phone-xr { top: 6px; } 
}       
      
.hamburger .bar,
.hamburger::after,
.hamburger::before { content: ''; display: block; width: 100%; height: 5px; background-color: #000; margin: 6px 0; transition: 0.5s;}
.hamburger.active:before { transform: rotate(-45deg) translate(-8px, 6px); }
.hamburger.active:after { transform: rotate(45deg) translate(-9px, -8px); }
.hamburger.active .bar { opacity: 0; } 