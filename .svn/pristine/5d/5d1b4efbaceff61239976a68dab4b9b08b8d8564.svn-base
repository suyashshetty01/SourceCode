                if (pg_get.hasOwnProperty('Output') && (pg_get['Output'].toString().toLowerCase().indexOf('|ccavenue|r') > -1 || pg_get['Output'].toString().toLowerCase().indexOf('|payu|r') > -1)) {//reliance
                    insurer_id = 9;
                    let arr_pg = pg_get['Output'].split('|');
