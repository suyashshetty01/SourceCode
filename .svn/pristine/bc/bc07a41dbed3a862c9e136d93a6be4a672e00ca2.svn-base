/* Single Handle Slider */
.slider{-webkit-appearance:none;width:100%;height: 4px;border-radius:5px;background: #d3d3d3;outline:none;opacity:0.7;-webkit-transition:.2s;transition:opacity .2s;}
.slider::-webkit-slider-thumb{-webkit-appearance:none;appearance:none;width:20px;height:20px;border-radius:50%;background:#2196f3;cursor:pointer;box-shadow: 0px 2px 7px 1px #999}
.slider::-moz-range-thumb{width:20px;height: 20px;border-radius:50%;background:#2196f3;cursor:pointer;}
.section_pane{font-size:13px;font-weight: bold}
.section_pane .left_label{float: left;}
.section_pane .right_label{float: right;}
.section_pane input{width: 100%;}

/*Radio box*/
.container{display:block;position:relative;padding: 0px 25px;cursor:pointer;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;font-weight: normal;}
.container input{position:absolute;opacity:0;cursor:pointer;}
.checkmark{position:absolute;top:2px;left:0;height:15px;width:15px;background-color:#fff;border-radius:50%;border:1px solid #2196f3}
.container:hover input ~ .checkmark{background-color:#fff;}
.container input:checked ~ .checkmark{background-color:#fff;}
.checkmark:after{content:"";position:absolute;display:none;}
.container input:checked ~ .checkmark:after{display:block;}
.container .checkmark:after{top:3px;left:3px;width:9px;height:9px;border-radius:50%;background:#2196f3;}
/* ToolTip */

.toolTipBox{position: absolute; background: rgba(0,0,0,0.9);color: #fff; padding:5px 9px; border-radius: 5px;z-index: 9999;font-size: 12px;max-width: 20%;margin: 0px auto;opacity: 1;animation-name: fadeInOpacity;animation-iteration-count: 1;animation-timing-function: ease-in;animation-duration: 0.3s; }
.tooltext_detail{display: none;}
/* Checkbox*/

.cbox{display:block;position:relative;padding-left:25px;cursor:pointer;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;font-weight: normal}
.cbox input[type="checkbox"]{position:absolute;opacity:0;cursor:pointer;}
.tick{position:absolute;top:2px;left:0;height:15px;width:15px;background-color:#fff;
    border:1px solid #2196f3}
.cbox:hover input[type="checkbox"] ~ .tick{background-color:#fff;}
.cbox input[type="checkbox"]:checked ~ .tick{background-color:#fff;}
.tick:after{content:"";position:absolute;display:none;}
.cbox input[type="checkbox"]:checked ~ .tick:after{display:block;}
.cbox .tick:after{top:3px;left:3px;width:9px;height:9px;background:#2196f3;}

/* Loader CSS */
#loader{
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 9999;
    background: rgb(255,255,255,0.4);
}
#loader img{
	width:100px;
	height: 100px;
	position: absolute;
	left: 50%;top: 50%;
	transform: translate(-50px, -50px);
}