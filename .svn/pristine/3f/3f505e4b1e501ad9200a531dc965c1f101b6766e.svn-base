input[type='range']::-webkit-slider-runnable-track {
    border-radius: 5px;
  }
  
  input[type='range']::-webkit-slider-thumb {
    width: 15px;
    height: 15px;
    cursor: pointer;
    background: #1A81FF;
    border: solid white 1px;
    border-radius: 50%;
    margin-top: 0px;
    box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.4);
  }
  
  /** FF*/
  
  input[type="range"]::-moz-range-progress {
    border-radius: 5px;
  }
  
  input[type="range"]::-moz-range-track {
    border-radius: 5px;
  }
  
  input[type="range"]::-moz-range-thumb {
    width: 15px;
    height: 15px;
    cursor: pointer;
    background: #1A81FF;
    border: solid white 1px;
    border-radius: 50%;
    margin-top: 0px;
    box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.4);
  }

.dropdown-menu-box{background: $whiteColor; box-shadow: 0px 0px 40px rgba(0, 0, 0, 0.15); border-radius: 10px; border: none; padding: 15px;}

.product-detail-row{@include dispflex(flex,flex-start,flex-start); flex-direction: row;
    .product-detail-col{margin-right: 5px;
        .common-dropdown{font-size: 15px; border: none; outline: none; font-family: $Helvetica-700; color: #222;background-image:url(../images/icons/dropdown-arrow.png); background-position:calc(100% - 8px) center; border-bottom: 1px solid #999;}
        &:last-child{width: 100%;
            .common-dropdown{width: 100%; text-overflow: ellipsis;}
        }
    }
    @include big-desktop{
        .product-detail-col .common-dropdown{max-width: 205px;}
    }
    @include mini-desktop{justify-content: flex-start;}
    @include tab{
        .product-detail-col:last-child .common-dropdown{max-width: 200px;}
    }
    @include phone-6{
        .product-detail-col{
            &:last-child .common-dropdown{background-position:calc(100% - 0px) center;}
            .detail-text{font-size: 14px}
        }
    }
    @include phone-5s{flex-wrap: wrap;}
}
.profile-detail-row{ @include dispflex(flex,flex-start,center); flex-direction: row; margin: 0 -3px; flex-wrap: wrap;
    .profile-detail-col{@include flex_box(3); padding: 0 3px; text-align: center; position: relative;
        &::after{content: ''; background: #DDD; position: absolute; right: 0; top: 0; display: inline-block; width: 1px; height: 16px;}
        &:last-child{
            &::after{display: none;}
        }
        &:first-child{
            &::after{top: 8px; right: -5px}
        }
    }
    @include big-desktop{
        .profile-detail-col:first-child::after{right: 0px;}
    }
    @include tab{
        .profile-detail-col{flex: 0 0 auto; max-width: 100%; margin-right: 20px;
            &::after{right: -10px;}
            &:first-child{
                &::after{right: -10px;}
            }
        }
    }
}

.filter-row{@include dispflex(flex,space-between,center); flex-direction: row; flex-wrap: wrap;
    .filter-col{
        &.flex-30{flex: 0 0 30%; max-width: 30%;}
    }
    @include mini-desktop{margin: 0 -8px;
        .filter-col{flex: 0 0 50%; max-width: 50%; padding: 0 8px; margin-bottom: 10px;
            &.flex-30{flex: 0 0 50%; max-width: 50%;}
            &:nth-last-child(-n+2){margin-bottom: 0px}
        }
    }
}
.filter-box{@include dispflex(flex,flex-start,center); flex-direction: row;
    .filter-title-box{margin-right: 5px;
        .filter-title-text{color: #666; font-size:15px; display: block; }
    }
    .filter-dropdown-box{position: relative;
        // .dropdown-icon{position: absolute; top: 0; right: 0;}
        .filter-dropdown-text{font-family:$Helvetica-700; font-size: 15px;}
    }
    @include phone-xr{
        .filter-title-box{
            .filter-title-text span{display: none;}
        }   
    }
    @include phone-xr{
        .filter-title-box{
            .filter-title-text{font-size: 14px;}
        }   
    }
    @include phone-6{
        .filter-dropdown-box{
            .filter-dropdown-text{font-size: 14px;}
        }
    }
}

.discounts-dropdown-box{
    inset: 0px auto auto -75px !important;
    max-height: 350px; overflow: auto; 
    &::-webkit-scrollbar { width: 5px; height: 5px; }
    &::-webkit-scrollbar-track { box-shadow: inset 0 0 5px rgba(26,129,255,0.6);} 
    &::-webkit-scrollbar-thumb { background: $primarycolor; border-radius: 10px;}
}

//Alert Modal Popup Design
.alert-popup-modal{
    .modal-dialog{max-width: 560px;
        .modal-content{
            .modal-body{padding: 30px;
                .alert-area{
                    .icon-box{height: 80px; width: 80px; border-radius: 50%; background: #FFEBE8; margin: 0 auto; margin-bottom: 30px; @include dispflex(flex,center,center);}
                    .alert-message-text{color: #CC1010; text-align: center; margin-bottom: 30px;}
                    .alert-area-buttons{justify-content: space-between;
                        .main-btn{padding: 17px 0px; font-family: $Helvetica-500; flex: 0 0 240px; max-width: 240px; box-shadow: 10px 10px 40px rgba(26, 129, 255, 0.1);
                            &:first-child{margin-right: 10px;}
                            &.light-btn{background: #EEEFF6; color: $extraLightText;}
                        }
                    }
                }
            }
        }
    }
    @include phone-xs{
        .modal-dialog{max-width: 450px;
            .modal-content .modal-body .alert-area .alert-area-buttons .main-btn{flex: 0 0 49%; max-width: 49%; padding-left: 5px; padding-right: 5px; font-size: 14px;
                &:first-child{margin: 0px;}
            }
        }
    }
    @include phone-xr{
        .modal-dialog{max-width: 400px;
            .modal-content .modal-body{padding: 20px;
                .alert-area{
                    .icon-box{height: 70px; width: 70px; margin-bottom: 20px;}
                    .alert-message-text{margin-bottom: 20px;}
                    .alert-area-buttons{flex-wrap: wrap;
                        .main-btn{flex: 0 0 100%; max-width: 100%;
                            &:first-child{margin-bottom: 10px;}
                        }
                    } 
                } 
            } 
        }
    }
    @include phone-6{
        .modal-dialog{max-width: 350px;
            .modal-content .modal-body .alert-area .icon-box{height: 60px; width: 60px; 
                img{height: 45px;}
            }
        }
    }
    @include phone-5s{
        .modal-dialog{max-width: 300px;}
    }
}


//Modal Open from Bottom
.modal-bottom-fixed{ overflow: hidden;
    &.fade{
        .modal-dialog{transform: translate(0,0px);}
    }
    .modal-button-area{@include dispflex(flex,flex-start,center); flex: 0 0 90px; max-width: 90px; }
    .modal-dialog{max-width: 100%; height: 100%; position: relative; margin: 0px; transform: translate3d(0, 100vh, 0);
        .modal-content{border-radius: 20px 20px 0px 0px; border: 0px; position: absolute; bottom: -100%; transition: 0.35s ease-in-out;
            .modal-header{border-bottom: 0px; padding: 20px;
                h3{font-size: 30px;}
            }
            .modal-body{padding: 20px; padding-top: 0px;}
        }
    }
    &.show{
        .modal-dialog{
            .modal-content{bottom: 0px;}
        }
    }
    @include phone-xs{
        .modal-dialog .modal-content .modal-header h3{font-size: 25px;}
    }
    @include phone-xr{
        .modal-dialog .modal-content .modal-header h3{font-size: 20px;}
    }
    @media screen and (max-height: 600px) {
        .modal-dialog .modal-content .modal-body{max-height: 450px; overflow-y: auto; overflow-x: hidden;}
    }
    @media screen and (max-height: 550px) {
        .modal-dialog .modal-content .modal-body{max-height: 420px; overflow-y: auto; overflow-x: hidden;}
    }
    @include landscape-mode{
        .modal-dialog .modal-content .modal-body{max-height: 300px; overflow-y: auto; overflow-x: hidden;}
    }
}
.modal-bottom-section{
    .mob-modal-filter-area{
        @include phone{
            .filter-row{
                .filter-col{flex: 0 0 100%; max-width: 100%; margin-bottom: 15px;
                    &:last-child{margin-bottom: 20px;}
                    .discounts-dropdown-box{inset: 0px auto auto 0px!important;}
                }
            }
            .dropdown-menu{
                &.show{position: static !important; transform: none !important;}
            }
        }
        @include phone-xs{
            .filter-row .filter-col .dropdown-toggle-custom{width: 100%;
                .filter-box{width: 100%; justify-content: space-between;}
            }
        }
        @include phone-5s{
            .filter-row .filter-col .dropdown-toggle-custom{
                &.additional-cover-dropdown{
                    .filter-box .filter-dropdown-box{max-width: 42px; overflow: hidden;}
                }
            }
        }
    }
    .mob-modal-detail-area{
        @include phone{
            .policy-detail-listing-row .policy-detail-listing-col{@include flex_box(3);}
        }
        @include phone-xs{
            .policy-detail-listing-row .policy-detail-listing-col{@include flex_box(2);}
        }
        @include phone-5s{
            .policy-detail-listing-row .policy-detail-listing-col{@include flex_box(1);}
        }
    }
}

.result-listing-select-row{@include dispflex(flex,center,flex-start); flex-direction: row; flex-wrap: wrap; margin: 0 -10px;
    .result-listing-select-col{@include flex_box(5); padding: 0 10px; margin-bottom: 20px;
        .result-listing-select-box{position: relative; padding: 10px;background: $whiteColor; box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.1); border-radius: 10px; min-height: 240px;}
    }
    @include mini-desktop{
        .result-listing-select-col{@include flex_box(4);}
    }
    @include tab{
        .result-listing-select-col{@include flex_box(3);}
    }
    @include phone{
        .result-listing-select-col{@include flex_box(2);
            .result-listing-select-box{box-shadow: 0px 0px 20px rgba(26, 129, 255, 0.15); min-height: auto;}
        }
    }
    @include phone-xr{
        .mob-none{display: none;}
    }
    @include phone-5s{
        .result-listing-select-col{@include flex_box(1);
            .result-listing-select-box{max-width: 220px; margin: 0 auto;}
        }
    }
} 

.custom-dropdown-box{padding: 0px; overflow: hidden;}
.dropdown-list-row{max-height: 170px; overflow: auto;
    .dropdown-list-col{ transition: $transitionCommon;
        .dropdown-list-link{color: $lightenGrey; padding: 15px;}
        &:hover{background: $primarycolor;
            .dropdown-list-link{color: $whiteColor;}
        }
        &.active{background: $primarycolor;
            .dropdown-list-link{color: $whiteColor;}
        }
    }
    @include phone-6{
        .dropdown-list-col .dropdown-list-link{padding: 10px; font-size: 13px !important;}
    }
}

//Insurance Image Box
.insurance-image-box{text-align: center; margin-bottom: 10px; min-height: 53px;
    @include phone-xr{
        img{height: 40px;}
    }
    @include phone-xr{min-height: 35px;
        img{height: 35px;}
    }
    @include phone-6{min-height: 30px;
        img{height: 30px;}
    }
    @include phone-5s{min-height: auto;
        img{height: auto;}
    }
}
//Price block
.price-block{border-radius: 10px; background-color: $primarycolor;padding: 10px; margin-bottom: 10px; transition: $transitionCommon;
    .price-tag{transform: translateX(0px); transition: $transitionCommon;}
    &:hover{ background-color: $primaryDark;
        .price-tag{transform: translateX(5px);}
    }
    @include phone-6{padding: 6px 5px;}
}

//Price Detail block
.price-detail-box{ @include dispflex(flex,center,center); flex-direction: row;}

//Value block
.value-block{background: #DEEDFF; border-radius: 10px; padding: 5px; margin-bottom: 10px;
    @include phone-xr{}
}

//Value Detail block
.value-detail-box{@include dispflex(flex,center,center); flex-direction: row;
    .value-title-text{color: $lightenGrey; font-family: $Helvetica-500; font-size: 14px; margin-right: 5px;}
    .price-value{font-family: $Helvetica-700;font-size: 14px;}
    @include phone-xr{flex-direction: column;
        .value-title-text,.price-value{font-size: 13px;}
    }
}

//Addons Added from dropdown
.value-block.addons-added{border-bottom-left-radius: 0;border-bottom-right-radius: 0; margin-bottom: 0px;}
.addons-area{display: none;}
.addons-area.addons-added{display: block; background: #F4F9FF; border-bottom-left-radius: 10px; border-bottom-right-radius: 10px; margin-bottom: 5px;
    .addons-box{padding: 4px 10px; justify-content: space-between; align-items: flex-start;
        .addons-text{font-size: 14px; color: #666; margin-right: 5px;}
        .addons-value{font-size: 14px; font-family:$Helvetica-700;}
    }
    @include phone-xr{
        .addons-box{
            .addons-text{font-size: 13px;}
            .addons-value{font-size: 13px;}
        }
    }
}
.depreciation-area.addons-added{display: none;}

//Add block
.add-block{background: #EBF4FF;border-radius: 5px; color: $primarycolor; font-family: $Helvetica-500;
    font-size: 12px; padding: 4px; text-align: center; flex: 0 0 50px; max-width: 50px; transition: $transitionCommon;
    &:hover{background: $primarycolor; color: $whiteColor;}
    @include phone-6{flex: 0 0 40px; max-width: 40px;}
    @include phone-5s{flex: 0 0 100%; max-width: 100%; width: 100%;}
}

.depreciation-area{margin-bottom: 10px;
    &.flex-box{justify-content: center;}
    @include phone-6{
        &.flex-box{justify-content: space-between;}
    }
    @include phone-5s{
        &.flex-box{flex-direction: column; justify-content: center; align-items: center;}
    }
}

//Description Box
.depreciation-box{flex: 0 0 auto; max-width: auto; margin-right: 5px;
    .depreciation-text{font-size: 12px; color: rgba(0,0,0,0.5);}
    .depreciation-value{font-size: 12px; font-family: $Helvetica-500;}
    @include phone-xr{
        .depreciation-value{font-size: 11px;}
        .depreciation-text{font-size: 11px; display: inline-flex;
            span{display: inline-block; margin-left: 3px; max-width: 20px; overflow: hidden;}
        }
    }
}

//Policy Detail Link
.policy-detail-link-box{font-size: 12px; font-family: $Helvetica-500; text-align: center;
    &:hover{color: $primarycolor; text-decoration: underline;}
}

//Price tag
.price-tag{width: 50px; height: 25px; background: url(../images/icons/price-tag.svg) center center no-repeat; background-size: 50px 20px; font-size: 9px;color: $whiteColor; @include dispflex(flex,center,center); flex-direction: row;
    @include big-desktop{background-size: 55px 20px; width: 55px; font-size: 10px;}
}
.price-text{font-family: $Helvetica-700; color: $whiteColor; margin-right: 5px;
    @include phone-6{margin-right: 3px}
}

//Policy Listing
.policy-detail-listing-row{@include dispflex(flex,flex-start,center); flex-direction: row; flex-wrap: wrap;
    .policy-detail-listing-col{@include flex_box(3); margin-bottom: 15px;
        .detail-box{@include dispflex(flex,flex-start,center); flex-direction: row;}
        .policy-titlt-text{color: #666; margin-right: 8px; font-size: 15px;}
        .policy-value-text{font-size: 15px}
    }
    @include mini-desktop{
        .policy-detail-listing-col{@include flex_box(2); margin-bottom:10px;}
    }
    @include tab{
        .policy-detail-listing-col{margin-bottom:5px;}
    }
}
.insurance-detail-box{
    .policy-detail-listing-row{
        .policy-detail-listing-col{
            &:nth-last-child(-n+3){margin-bottom: 0px}
        }
    }
}


.blue-border-box{border: 1px solid $primarycolor; border-radius: 5px;text-align: center; padding: 2px 0px; transition: $transitionCommon;
    .icon-box img{filter: none; transition: $transitionCommon;}
    &.edit-box{flex: 0 0 60px; max-width: 60px;font-size: 15px; font-family: $Helvetica-500; color: $primarycolor;}
    &.share-box{flex: 0 0 80px; max-width: 80px; margin-right: 10px;}
    &.flex-box{justify-content: center;}
    .inner-text{font-size: 15px; font-family: $Helvetica-500; color: $primarycolor; transition: $transitionCommon;}
    .icon-box{margin-right: 5px; 
        img{margin-bottom: 4px;}
    }
    &:hover{background: $primarycolor; color: $whiteColor;
        .inner-text{color: $whiteColor;}
        .icon-box img{filter: invert(50%) brightness(2);}
    }
}

.mob-fixed-section{position: fixed; bottom: 0; left: 0; right: 0; width: 100%; display: none; z-index: 2;
    &.detail-and-filter-section{
        .detail-and-filter-area{background: $whiteColor; padding: 5px 0px;}
    }
    @include phone{display: block;}
}

.detail-and-filter-section + .footer-section{
    @include phone{padding-bottom: 100px;}
}

.detail-and-filter-box{@include dispflex(flex,space-between,center); flex-direction: row;
    .detail-modal-box{flex: 0 0 50%; max-width: 50%;}
    .filter-modal-box{flex: 0 0 50%; max-width: 50%; border-left: 1px solid #ddd;}
    .modal-open-btn{@include dispflex(flex,center,center); flex-direction: row; background: $whiteColor; width: 100%; padding: 13px 0px; color: $lightenText;
        .icon-box{margin-right: 10px;}
        .title-text{display: block; font-size: 16px; font-family: $Helvetica-500; color: $lightenText;}
    }
    @include phone-6{
        .modal-open-btn .title-text{font-size: 14px;}
    }
}

.dropdown-toggle{
    &::after{transition: $transitionCommon;}
    &.show{
        &.dropdown-toggle::after{transform: rotate(180deg);}
    }
}

.mob-profile-detail-section{position: fixed;bottom: 80px;right: 20px; display: none;
    @include phone{display: block;}
}
//Profile
.profile-box-area{position: relative; z-index:2; left: 0; transition: $transitionCommon;
    .profile-button{ border-radius: 50%; overflow: hidden; border:  2px solid $whiteColor; position: relative;
        img{height: 50px;}
    }
    .profile-box{ position: absolute; z-index:0; left:0%;top:0px; bottom: 0; margin:auto;min-width:0px; transition:0.5s ease-in-out; opacity:0; border-radius: 25px; background: $whiteColor; box-shadow: 0px 4px 30px rgba(0, 0, 0, 0.2);
        .profile-detail-area{opacity: 0;
             .profile-detail{position: relative; display: none; 
                 &::before{content: ''; position: absolute; left: -8px; top: 3px; height: 90%; width: 1px; background: #ddd;}
                 .pfile-title{font-size: 15px; font-family: $Helvetica-700;}
                 .profile-description-box{line-height: 15px;
                     .profile-description-title{font-size: 14px; color: #666;}
                     .profile-description-detail{font-size: 14px; color: #666; font-family: $Helvetica-700;}
                 }
             }
             &.open{opacity: 1;}
          }
        }
    &.profile-active{
        left: -150px;
        .profile-detail-area{
            .profile-detail{display: block;}
        }
        .profile-box{min-width:200px; opacity:1; padding-left: 70px;padding-top: 5px; padding-bottom: 5px;}  
    }
}

//Result box style
.result-title-box{@include dispflex(flex,space-between,center); flex-direction: row; margin-bottom: 15px;
    .help-icon{cursor: pointer;
        img{height: 18px;}
    }
    @include phone{
        .title-text{font-size: 22px}
    }
}
.result-filter-box-area{ @include dispflex(flex,flex-end,center); flex-direction: row; flex: 0 0 40%; max-width: 40%;}

//Profile box style
.profile-box{@include dispflex(flex,center,flex-start); flex-direction: row; align-items: center;
    .image-box{height: 35px; width: 35px; border-radius: 50%; margin-right: 8px; overflow: hidden;
        img{width: 100%;}
    }
    @include big-desktop{justify-content: flex-start;}
    @include tab{justify-content: flex-start;}
}
.profile-detail-text{color: #666; font-family: $Helvetica-500; font-size: 14px;
    .profile-detail-value{color: $blackColor; font-family: $Helvetica-500;}
}
.profile-holder-name{color: $blackColor;}

//Auto complete menu style

.ui-menu.search-autocomplete{ border: 0px; background: $whiteColor; height: 180px; overflow-x: hidden; overflow-y: auto; padding-top: 10px;
        .ui-menu-item{padding: 10px 15px; background: $whiteColor !important; cursor: pointer;
            .ui-menu-item-wrapper{font-size: 16px; font-family: $Helvetica-500;background-color: transparent !important; border-color: #fff !important; color: #222 !important;}
            &:hover{box-shadow: 0px 5px 36px rgba(0,0,0,0.2) !important; transform: scale(1.01);
                .ui-menu-item-wrapper{background: $whiteColor; color: #444; border: 0px !important;
                    &:hover{background-color: $whiteColor;}
                }
            }
            &:hover{background-color: $whiteColor;}
        }
    }
    .ui-menu-item{padding: 10px 15px; background: $whiteColor !important;   
        .ui-menu-item-wrapper{font-size: 16px; font-family: $Helvetica-500;}
        &:hover{box-shadow: 0px 5px 36px rgba(0,0,0,0.2) !important; transform: scale(1.01);
            .ui-menu-item-wrapper{background: $whiteColor; color: #444; border: 0px !important;
                &:hover{background-color: $whiteColor;}
            }
        }
        &:hover{background-color: $whiteColor;}
    }
    @include phone-xr{
        .ui-menu.search-autocomplete{
            &.search-autocomplete{width: 190px !important;
                .ui-menu-item{padding: 10px;}
            }
        }
    }
    @include phone-6{
        .ui-menu.search-autocomplete{
            &.search-autocomplete{width: 160px !important;
                .ui-menu-item{padding: 10px 5px;
                    .ui-menu-item-wrapper{font-size: 14px;}
                }
            }
        }
    } 
    @include phone-5s{
        .ui-menu.search-autocomplete{
            &.search-autocomplete{width: 135px !important;
                .ui-menu-item{
                    .ui-menu-item-wrapper{font-size: 12px;}
                }
            }
        }
    }

//tag field
.tag-field-box{position: relative;
    .tag-field{position: relative; z-index: 2;}
    &::after{ content: ''; position: absolute; top: -5px; left: 0; right: 0; margin: 0 auto;
        width: 95%; min-height: 260px; border-radius: 12px; background: #fff; box-shadow: 0px 5px 36px rgba(0, 0, 0, 0.2); z-index: 1; display: none;
    }
    &.active{
        &::after{display: block;}   
    } 
    @include phone-xr{
        &::after{width: 100%;}
    }
}

//Checkbox Select
.check-box-select{position: absolute; top: 5px; right: 10px;}

.radio-items-box{
    .radio-title{display: block; font-family: $Helvetica-500; margin-bottom: 8px;}
    .radio-item-row{margin-bottom: 10px;
        &:last-child{margin-bottom: 15px;}
    }
} 
//Radio Select
.radio-select-box{@include dispflex(flex,flex-start,center); flex-direction: row; flex-wrap: wrap; margin-bottom: 20px;
    .radio-box{margin-right: 20px;}
}

//Policy Information Modal
#policyInfromationModal{
    .modal-body{max-height: 600px; overflow-y: auto; overflow-x: hidden;}
    @include phone{
        .modal-body{max-height: 100%;}
    }
}
.policy-information-area{
    .radio-title{display: block; margin-bottom: 10px;}
    .no-claim-bonus-info-box{
        .radio-select-box .radio-box{flex: 0 0 25%; max-width: 25%; margin-bottom: 12px;}
    }
}


// .NewCar-modal{
//     @include phone{height: 100vh;}
// }
.modal-single-view{
    .modal-profile-box{flex: 0 0 38%; max-width: 38%; margin-left: auto;}
    .modal-profile-box + .btn-close{margin-left: 0px;}
    @include phone{
        .WithoutCarNumber-modal{height: 100vh;}
        .NewCar-modal{height: 100vh;}
        .modal-profile-box{order: 1; flex: 0 0 auto; max-width: 100%; margin-left: auto;}
        .title-box{order: 2}
        .btn-close{order: 3;}
    }
    @include phone-6{
        .view-quote-btn{min-height: 46px; height:46px; @include dispflex(flex,center,center);}
    }
}

.modal-single-view{
    .modal-content{min-width: 500px; max-height: 605px;
