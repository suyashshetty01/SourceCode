/* checkbox */
.checkbox label {
    padding-left: 30px;
    font-size: 14px;
}

input[type="checkbox"] {
    display: none;
}

    input[type="checkbox"] + .box:before,
    input[type="checkbox"] + .box:after {
        transition: all 0.3s;
        animation: radiorippleeffect 0.2s linear forwards;
    }

    input[type="checkbox"] + .box:after {
        position: absolute;
        left: 4px;
        top: 3px;
        margin-top: -5px;
        margin-left: -4px;
        display: inline-block;
        content: ' ';
        width: 20px;
        height: 20px;
        border: 2px solid #999;
        border-radius: 2px;
        background-color: #fff;
        z-index: 1000;
    }

    input[type="checkbox"]:checked + .box:after {
        background-color: #009ee3;
        border-color: #009ee3;
    }

    input[type="checkbox"]:checked + .box:before {
        transform: rotate(45deg);
        position: absolute;
        left: 7px;
        top: 0px;
        width: 6px;
        height: 13px;
        border-width: 2px;
        border-style: solid;
        border-top: 0;
        border-left: 0;
        border-color: #fff;
        content: '';
        z-index: 10000;
    }
/* end checkbox */

/* radio button */
@keyframes radiorippleeffect {
    0% {
        box-shadow: 0px 0px 0px 1px transparent;
    }

    50% {
        box-shadow: 0px 0px 0px 15px rgba(0, 0, 0, 0.1);
    }

    100% {
        box-shadow: 0px 0px 0px 15px transparent;
    }
}

.md-radio {
    margin: 17px 0 15px 8px;
}

    .md-radio.md-radio-inline {
        display: block;
    }

    .md-radio input[type="radio"] {
        display: none;
    }

        .md-radio input[type="radio"]:checked + label:before {
            border-color: #009ee3;
            animation: radiorippleeffect 0.2s linear forwards;
            color: #000;
        }

        .md-radio input[type="radio"]:checked + label:after {
            transform: scale(1);
        }

    .md-radio label {
        display: block;
        height: 0px;
        position: relative;
        padding: 2px 0 0 25px;
        margin-bottom: 0;
        cursor: pointer;
        vertical-align: bottom;
        pointer-events: auto;
        left: 0px;
        color: #9c9c9c;
    }

    .md-radio:hover label {
        display: block;
        height: 0px;
        position: relative;
        padding: 2px 0 0 25px;
        margin-bottom: 0;
        cursor: pointer;
        vertical-align: bottom;
        pointer-events: auto;
        left: 0px;
        color: #232323;
    }

    .md-radio label:before, .md-radio label:after {
        position: absolute;
        content: '';
        border-radius: 50%;
        transition: all .3s ease;
        transition-property: transform, border-color;
    }

    .md-radio label:before {
        left: 0;
        top: 0;
        width: 20px;
        height: 20px;
        border: 2px solid rgba(0, 0, 0, 0.54);
    }

    .md-radio label:after {
        top: 5px;
        left: 5px;
        width: 10px;
        height: 10px;
        transform: scale(0);
        background: #009ee3;
    }

*, *:before, *:after {
    box-sizing: border-box;
}
