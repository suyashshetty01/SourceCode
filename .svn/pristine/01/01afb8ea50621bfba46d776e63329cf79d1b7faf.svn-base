            .location-listing{
                .location-listing-row{@include dispflex(flex,flex-start,flex-start); flex-direction: row; margin-bottom: 40px;
                    &::before{content: ''; background: url(../images/icons/location.png) no-repeat center center; display: inline-block; margin-right: 5px; flex:0 0 20px; max-width: 20px; height: 22px; margin-top: 2px;}
                    .location-listing-box{
                        .title-text{margin-bottom: 5px;}
                    }
                }
            }
            .location-image-area{padding-left: 25px;
                .location-image-box{border-radius: 10px; overflow: hidden; 
                    img{height: 260px; width: 100%; object-fit: cover;} 
                 }
            }
        }
    }
    @include big-desktop{
        .contact-location-section .location-area .location-image-area .location-image-box {
            img{height: 285px;}
        }
    }
    @include mini-desktop{
        .contact-location-section .location-area .location-image-area .location-image-box {
            img{height: 200px;}
        }
    }
    @include tab{
        .contact-location-section .location-area{
            .location-listing .location-listing-row{margin-bottom: 30px;}
            .location-image-area .location-image-box {
                img{height: 150px;}
            }
        } 
        .contact-form-area .contact-form-box{padding: 20px;
            .contact-form{
                .contact-form-inner{margin: 0 -10px; align-items: flex-end;
                    .input-field-box{padding: 0 10px;}
                }
                .submit-btn{min-width: 130px;}
            } 
        }
    }
    @include phone{
        .contact-location-section .location-area .location-image-area{padding: 0px;
            .location-image-box img{height: 240px;}
        } 
        .contact-form-area .contact-form-box{box-shadow: none; border-radius: 0px; padding: 0px; margin-bottom: 40px;
            .submit-btn{width: 100%;}
        }
    }
    @include phone-xr{
        .contact-form-area .contact-form-box{
            .contact-form{
                .contact-form-inner .input-field-box{flex:0 0 100%; max-width: 100%;                
                    &.mob-flex-50{flex: 0 0 50%; max-width: 50%;}
                }
            } 
        }
        .contact-location-section .location-area .location-image-area .location-image-box img{height: 200px;}
