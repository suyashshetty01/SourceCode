//Offer section style
.offer-section{ 
    // .sub-title{margin-bottom: 30px; text-align: center;}
    .card-listing-row{display: flex; flex-direction: row;
        .card-listing-col{flex: 0 0 33.33%; max-width: 33.33%;}
    }
    .card-main{padding: 30px; border: 5px solid transparent; transition: $transitionCommon; cursor: pointer;  min-height: 100%;
        //  min-height: 455px;
        .card-box{text-align: center;
            .image-box{margin-bottom: 20px;transition: $transitionCommon; transform: scale(1);
                img{height: 275px; }
            }
            .card-detail-box{ position: relative; top:0px; transition: $transitionCommon;
                .card-heading{margin-bottom: 20px;}
                .description{opacity: 0; transition: $transitionCommon;}
            }
        }
        &.bg-cream{background: #FFF3F2;}
        &.bg-light-blue{background: #D8F0FD;}
        &.bg-light-pink{background: #F6EBFF;}
        &:hover{border: 5px solid $whiteColor; box-shadow: 0px 3px 60px rgba(0, 0, 0, 0.25);     transform: scale(1.01);
            .card-box{
                .image-box{transform:scale(0.73) translateY(-50px);}
                // .image-box img{transform: scale(0.75);} 
                .card-detail-box{ top:-50px;
                    .description{opacity: 1;}
                }
            } 
        }
        &:last-child{
            &:hover{transform: scale(1);}   
        }
    }
    @media screen and (min-width:1900px){
        .card-listing-row{max-width: 1320px; margin: 0 auto;}
    }  
    @include mini-desktop{
        .card-main{padding: 25px;}
    }
    @include tab{
        .card-main{padding: 20px;
            .card-box{
                .image-box {margin-bottom: 10px;
                    img{height: 200px; object-fit: cover;}
                }
                .card-detail-box .card-heading{margin-bottom: 15px;}
            } 
            &:hover{
                .card-box{
                    .image-box{transform: scale(0.85) translateY(-30px);}
                    // .image-box img{height: 170px;}
                } 
                .card-detail-box{top: -30px;}
            }
        } 
    }
    @include phone{
        .card-listing-row{flex-wrap: wrap;
            .card-listing-col {
                flex: 0 0 100%;
                max-width: 100%;
            }
        } 
        .card-main{ min-height: auto;
            .card-box{@include dispflex(flex,center,center);
                .image-box{flex: 0 0 30%; max-width: 30%; margin-right: 20px;
                    img{height: 180px; width: 100%; object-fit: contain;}
                }
                .card-detail-box{flex: 0 0 70%; max-width: 70%; text-align: left;
                    .card-heading br{display: none;}
                    .description{opacity: 1;}
                }
            }
            &:hover{
                .card-box{
                    .image-box {transform: none;}
                    .card-detail-box{top: 0px;}
                } 
                
            }
        }
    }
    @include phone-xs{
        .card-main{padding: 15px;
            .card-box{
                .image-box{margin-right:15px;
                    img{height: 150px;}
