//Blogs & Videos section style
.blogs-and-videos-section{position: relative;
    .blog-bg-graphics-box{position: absolute;
        &.left-bg-box{top: 30px; left: 60px;}
        &.right-bg-box{top: 250px; right: 60px;}
    } 
    // .sub-title{margin-bottom: 30px;}
    .blogs-area{padding: 40px 0px; 
        .blog-box-row{@include dispflex(flex,flex-start,center); margin: 0 -15px;
            .blog-box-col{@include flex_box(3); padding: 0 15px;}
        }
    }
    .videos-area{
        .slider-wrap{
            // max-width: 1090px; margin: 0 auto;
            .slider-col{
                .slider-box{ 
                    .js-videos{margin: 0 -5px;
                        .slider-inner{ padding: 0 5px;
                            .slider-content-box{transform: scale(0.9); transition: $transitionCommon; cursor: pointer;
                                .tag-box{top: 10px; right: 10px; border-radius: 5px; padding: 1px 9px;
                                    .tag-text{font-size: 13px;}
                                }
                                .video-box{border-radius: 20px; overflow: hidden; position: relative;
                                    display: flex; flex-direction: row; box-shadow: 7px 7px 20px rgba(65,91,122,0.15);
                                    .video-player{width: 100%;
                                        .video-element{width: 100%; height: 170px; object-fit: cover; transition: $transitionCommon;}
                                    }
                                    .play-button{opacity: 0; transition: $transitionCommon; position: absolute; top: 0; left: 0; right: 0; bottom: 0; @include dispflex(flex,center,center);width: 50px; height: 50px; border-radius: 50%; margin: auto; z-index: 2;}
                                    .description-box{position: absolute; bottom: 10px; left: 10px; z-index: 2;
                                        .description-text{color: $whiteColor; font-family: $Helvetica-500;}
                                    }
                                    &::after{content: ''; position: absolute; bottom: 0; left: 0; height: 70%; width: 100%; background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, #000000 100%);}
                                }
                                .mob-description-box{display: none;}
                            } 
                            &:hover{
                                .slider-content-box{ transform: scale(1);
                                    .video-box{
                                        .play-button{opacity: 1;}
                                    } 
                                } 
                            }
                        } 
                    } 
                }
                .slick-arrow{
                    position: absolute; top: 0;bottom: 0;margin: auto;z-index: 2;
                    background-color: $whiteColor !important;box-shadow: 10px 10px 40px rgba(0, 0, 0, 0.1);
                    width: 40px;height: 40px;border-radius: 50%;font-size: 0px;border: 0px;
                    &.slick-prev{left: -50px;background: url(../images/icons/left_arrow.png) center     center no-repeat; background-size: 9px 15px; background-position: 13px 13px; background-repeat: no-repeat;
                    }
                    &.slick-next{ right: -50px; background: url(../images/icons/right_arrow.png) center center no-repeat; background-size: 9px 15px; background-position: 16px 13px; background-repeat: no-repeat;
                    }
                }
            }
        }
    }
    @include big-desktop{
        .videos-area .slider-wrap{ max-width: 1290px; margin: 0 auto;}
    }
    @include mini-desktop{
        .videos-area .slider-wrap{max-width: 910px;
            .slider-col .slick-arrow{width: 35px; height:35px; 
                &.slick-prev{left: -42px; background-position: 11px 10px;}
                &.slick-next{right: -42px; background-position: 14px 10px;}
            }
        } 
    }
    @media screen and (max-width:1024px){
        .videos-area .slider-wrap{max-width: 890px;}
    }
    @include tab{
        .videos-area .slider-wrap{max-width: 680px;
            .slider-col{
                .slick-arrow{ width: 30px; height: 30px;
                    &.slick-prev{left: -35px; background-size: 8px 15px; background-position: 10px 8px; }
                    &.slick-next{right: -35px; background-size: 8px 15px; background-position: 12px 8px;}
                }
                .slider-box .js-videos .slider-inner .slider-content-box .video-box{
                    .play-button{width: 40px; height: 40px;
                        .icon-box img{height: 15px;}
                    }
                    .description-box .description-text{font-size: 14px;}
                } 
            } 
        }
    }
    @include phone{
        .blogs-area{padding: 0px;
            .blog-box-row{flex-direction: column;
                .blog-box-col{flex: 0 0 100%; max-width: 100%; margin-bottom: 15px;}
            }
        } 
        .blog-bg-graphics-box{
            img{height: 120px;}
            &.left-bg-box{top: 220px;}
            &.right-bg-box{top: auto; bottom: 250px;}
        } 
        .videos-area{
            .slick-list{padding:0 10% 0 0;}
            .slider-col .slick-arrow{display: none !important;}
        }
    } 
    @include phone-xs{
        .blogs-area .blog-box-row{margin: 0px;
            .blog-box-col{padding: 0 15px}
        }
        .blog-bg-graphics-box{
            &.left-bg-box{left: 5px;}
            &.right-bg-box{right: 5px; bottom: 285px;}
        } 
        .videos-area{
            .slider-wrap{max-width: 100%; padding: 0 15px;
                .slider-col .slider-box .js-videos .slider-inner .slider-content-box{
                    .video-box .description-box{display: none;}
                    .mob-description-box{display: block; padding:5px;
                        .description-text{font-size: 13px;color: #666;}
                    }
                } 
            } 
        }
    }
    @include phone-xr{
        .tabs-main-section{
            .nav-tab-line{
                .nav-item{flex: 0 0 50%; max-width: 50%; padding: 0px;
                    .nav-link{width: 100%;}
